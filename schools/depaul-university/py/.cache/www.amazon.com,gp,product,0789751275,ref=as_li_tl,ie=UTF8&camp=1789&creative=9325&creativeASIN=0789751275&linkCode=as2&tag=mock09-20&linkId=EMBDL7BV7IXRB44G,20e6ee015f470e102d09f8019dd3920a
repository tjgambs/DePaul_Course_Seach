D%22MARGIN%3A%200px%22%3EWhen%20not%20consulting%2C%20teaching%2C%20or%20writing%2C%20Michael%20enjoys%201960s%20muscle%20cars%20and%20has%20a%20slot%20in%20his%20garage%20for%20a%20new%20project%20car.%3C%2FP%3E%20%20%3CP%20style%3D%22MARGIN%3A%200px%22%3E%26nbsp%3B%3C%2FP%3E%20%20%3CP%20style%3D%22MARGIN%3A%200px%22%3EYou%20can%20reach%20Michael%20by%20email%20at%20MikeG%40thesolutionfirm.com.%3C%2FP%3E%0A%20%20%20%20%20%20%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fdiv%3E%0A%3C%2Fdiv%3E%0A%0A%0A%0A%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fbody%3E%0A%3C%2Fhtml%3E%0A";
    obj.onloadCallback = onloadCallback;
    obj.needWidthAdjust = needWidthAdjust;
    var iframe = obj.createIframe (obj.iframeId, obj.iframeload);
    var wrapper = d.getElementById(obj.appendTo);
    wrapper.innerHTML = '';

    var anchor = d.createElement('div');
    anchor.id = 'biss-product-description-and-details';
    wrapper.appendChild(anchor);

    iframe = wrapper.appendChild(iframe);
    var doc;
    try {
      doc = iframe.contentWindow.document;
    } catch (e) {
      try {
        doc = iframe.contentDocument;
      } catch (ex) {}
    }
    if (doc && doc.open) {
      obj.doc = doc;
      doc.open().write(decodeURIComponent(iframeContent));
      doc.close();
    }
  }
  obj.resizeIframe = function() {
    if(hasCustomWidth) {
      setIframeWidth(obj.iframeId);
    }
    nTries = 1;
    setIframeHeight(obj.iframeId);
  }
}).apply(ProductDescriptionIframeResize);

amznJQ.onCompletion('amznJQ.criticalFeature', function () {
  amznJQ.available('jQuery',function(){
    setTimeout(function() {
      var iframeLoadFn = function() {
        var $ = jQuery;
        $("#pd-available").trigger("pdAvailable");
        $(window).resize(ProductDescriptionIframeResize.resizeIframe);
      };
      ProductDescriptionIframeResize.initialize(iframeLoadFn, 1);
    }, 50);
  });
});
</script>




#rhf-container #rhf-recs-down-text {
    padding-top: 13px;
    font-size: 16px;
    line-height: 26px;
}

#rhf-container #rhfNoRecsMessage {
    color: #666666;
    font-size: 13px;
    font-family: Arial;
}

* html .rhf {
    height:1%;
}

(window.AmazonUIPageJS ? AmazonUIPageJS : P).when('cf').execute(function() {
  (window.AmazonUIPageJS ? AmazonUIPageJS : P).load.js('http://z-ecx.images-amazon.com/images/I/31hkcW9EzqL._RC|11pTncR1UdL.js,210WcCBb7hL.js,31hNSbdr3VL.js,11xcysJtH+L.js_.js#AUIClients/BooksDetailPageMetaAsset');

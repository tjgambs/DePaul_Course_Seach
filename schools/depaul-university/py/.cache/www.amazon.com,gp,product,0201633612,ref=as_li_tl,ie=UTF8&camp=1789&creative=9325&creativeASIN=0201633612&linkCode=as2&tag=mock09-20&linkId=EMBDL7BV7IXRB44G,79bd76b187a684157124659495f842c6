    #minimalImageBlock #mainImageContainer { min-height: 200px; line-height: 200px; } 
    #minimalImageBlock #imgBlkFront { box-shadow: 0 2px 5px 1px rgba(0,0,0,0.2); }
    #minimalImageBlock #imgBlkFront { vertical-align: middle; }

e-col%20%7B%0A%20%20%20%20width%3A%2098.8827%25%3B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.last%20%7B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20100%25%3B%0A%20%20%20%20min-width%3A%20895px%3B%0A%7D%0A%0A*%20html%20%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20expression((document.body.clientWidth%20%3C%20936)%20%3F%20%22895px%22%20%3A%20%22100%25%22%20)%3B%0A%7D%0A%0A%20%20%20%20%3C%2Fstyle%3E%0A%20%20%20%20%3Cbase%20target%3D'_top'%20%2F%3E%0A%20%20%3C%2Fhead%3E%0A%20%20%3Cbody%3E%0A%20%20%3Cdiv%20id%3D%22pdIframeContent%22%3E%0A%0A%0A%0A%0A%20%0A%0A%0A%0A%0A%3Cdiv%20id%3D%22biss-product-description-and-details%22%3E%3C%2Fdiv%3E%0A%0A%3Cdiv%20class%3D%22bucket%22%20id%3D%22productDescription%22%3E%0A%20%20%3Ch2%3EEditorial%20Reviews%3C%2Fh2%3E%0A%0A%3Cdiv%20class%3D%22content%22%3E%0A%0A%0A%20%20%20%20%0A%20%20%20%20%20%20%20%3Ch3%20class%3D%22productDescriptionSource%22%3EReview%3C%2Fh3%3E%0A%20%20%20%20%20%20%20%3Cdiv%20class%3D%22productDescriptionWrapper%22%3E%0A%20%20%20%20%20%20%20%3CP%3EThis%20essential%20book%20is%20a%20tool%20for%20unlocking%20the%20power%20of%20Processing.%20With%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20this%20completely%20revised%20edition%2C%20Casey%20Reas%20and%20Ben%20Fry%20show%20readers%20how%20to%20use%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20Processing%20for%20thinking%2C%20making%2C%20and%20doing.%20This%20remarkable%20software%20environment%20has%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20opened%20the%20world%20of%20code%20to%20designers%2C%20architects%2C%20musicians%2C%20and%20animators.%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20Providing%20a%20powerful%20alternative%20to%20proprietary%20software%2C%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%3CI%3EProcessing%3C%2FI%3E%20speaks%20to%20self-education%20and%20networked%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20engagement.%3C%2FP%3E%20(Ellen%20Lupton%2C%20Director%20of%20the%20graphic%20design%20MFA%20program%20at%20Maryland%20Institute%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20College%20of%20Art%2C%20Baltimore%2C%20and%20author%20of%20%3CI%3EThinking%20with%20Type%3C%2FI%3E%20and%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%3CI%3EType%20on%20Screen%3C%2FI%3E)%3Cbr%20%2F%3E%3Cbr%20%2F%3E%3CP%3EProcessing%20has%20unlocked%20the%20potential%20of%20software%20as%20a%20creative%20medium%20by%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20integrating%20a%20programming%20language%20and%20development%20environment%20and%20linking%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20computation%20and%20the%20visual%20arts.%20This%20revised%20handbook%20provides%20expertly%20designed%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20and%20invaluable%20tutorials%20that%20introduce%20the%20syntax%20and%20concepts%20of%20software%20and%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20position%20it%20in%20the%20field%20of%20arts.%20Interviews%20with%20renowned%20artists%20give%20insight%20into%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20the%20creation%20of%20their%20landmark%20software%20projects%2C%20illustrating%20how%20programming%20is%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20applied%20in%20art.%3C%2FP%3E%20(Christiane%20Paul%2C%20Adjunct%20Curator%20of%20New%20Media%20Arts%2C%20Whitney%20Museum%20of%20American%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20Art)%3Cbr%20%2F%3E%3Cbr%20%2F%3E%3CP%3EIn%20addition%20to%20what%20you%20can%20expect%20--%20a%20great%20compendium%20explaining%20the%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20software's%20features%20and%20applications%20--%20the%20second%20edition%20of%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%3CI%3EProcessing%3C%2FI%3E%20comes%20with%20a%20well-curated%20series%20of%20interviews%20with%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20artists%20and%20designers%20for%20whom%20software%20is%20key%20to%20their%20work.%20These%20exceptional%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20insights%20into%20artistic%20practice%20contribute%20to%20the%20writing%20of%20history%20of%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20software-based%20art%20and%20design%20and%20contextualize%20Processing%20in%20an%20adequate%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20way.%3C%2FP%3E%20(Joachim%20Sauter%2C%20University%20of%20the%20Arts%20Berlin%2C%20Founder%20and%20Creative%20Director%20of%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20ART%2BCOM)%0A%20%20%20%20%20%20%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%0A%20%20%20%20%0A%20%20%20%20%20%20%20%3Ch3%20class%3D%22productDescriptionSource%22%3EAbout%20the%20Author%3C%2Fh3%3E%0A%20%20%20%20%20%20%20%3Cdiv%20class%3D%22productDescriptionWrapper%22%3E%0A%20%20%20%20%20%20%20Casey%20Reas%20is%20Professor%20of%20Design%20Media%20Arts%20at%20UCLA%20and%20coauthor%20of%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%3CI%3EProcessing%3A%20A%20Programming%20Handbook%20for%20Visual%20Designers%20and%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20Artists%3C%2FI%3E%20(MIT%20Press%2C%202007).%3Cbr%20%2F%3E%3Cbr%20%2F%3EBen%20Fry%20is%20Principal%20of%20Fathom%2C%20a%20design%20and%20software%20consultancy%20in%20Boston.%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20%20Together%2C%20Reas%20and%20Fry%20cofounded%20Processing%20in%202001.%0A%20%20%20%20%20%20%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fdiv%3E%0A%3C%2Fdiv%3E%0A%0A%0A%0A%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fbody%3E%0A%3C%2Fhtml%3E%0A";
    obj.onloadCallback = onloadCallback;
    obj.needWidthAdjust = needWidthAdjust;
    var iframe = obj.createIframe (obj.iframeId, obj.iframeload);
    var wrapper = d.getElementById(obj.appendTo);
    wrapper.innerHTML = '';

    var anchor = d.createElement('div');
    anchor.id = 'biss-product-description-and-details';
    wrapper.appendChild(anchor);

    iframe = wrapper.appendChild(iframe);
    var doc;
    try {
      doc = iframe.contentWindow.document;
    } catch (e) {
      try {
        doc = iframe.contentDocument;
      } catch (ex) {}
    }
    if (doc && doc.open) {
      obj.doc = doc;
      doc.open().write(decodeURIComponent(iframeContent));
      doc.close();
    }
  }
  obj.resizeIframe = function() {
    if(hasCustomWidth) {
      setIframeWidth(obj.iframeId);
    }
    nTries = 1;
    setIframeHeight(obj.iframeId);
  }
}).apply(ProductDescriptionIframeResize);

amznJQ.onCompletion('amznJQ.criticalFeature', function () {
  amznJQ.available('jQuery',function(){
    setTimeout(function() {
      var iframeLoadFn = function() {
        var $ = jQuery;
        $("#pd-available").trigger("pdAvailable");
        $(window).resize(ProductDescriptionIframeResize.resizeIframe);
      };
      ProductDescriptionIframeResize.initialize(iframeLoadFn, 1);
    }, 50);
  });
});
</script>




	    function showSLF() {
	         if(!feedbackFormAvailable) {
	           // Make a request to the handler with new ads 
	           $.post('/gp/product/aui_feedback_form_handler.html', { 'sid': '175-9698702-1815309','jsonAds' : escape(jsonAds) } ,function(data) {

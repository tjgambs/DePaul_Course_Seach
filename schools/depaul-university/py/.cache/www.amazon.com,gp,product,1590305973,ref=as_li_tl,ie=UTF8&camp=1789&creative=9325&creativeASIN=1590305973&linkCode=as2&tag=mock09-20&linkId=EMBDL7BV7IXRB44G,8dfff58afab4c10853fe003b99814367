        'frontImageHeight' : 295,
        'frontImageAspectRatio' : 0.8813559322033898,
        'imageGalleryData' : [{"mainUrl":"http://ecx.images-amazon.com/images/I/41323x--EBL.jpg","dimensions":[441,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/41323x--EBL._AC_SX75_CR,0,0,75,75_.jpg"},{"mainUrl":"http://ecx.images-amazon.com/images/I/51lm3HMEaAL.jpg","dimensions":[441,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/51lm3HMEaAL._AC_SX75_CR,0,0,75,75_.jpg"},{"mainUrl":"http://ecx.images-amazon.com/images/I/31lPhnNw-QL.jpg","dimensions":[375,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/31lPhnNw-QL._AC_SX75_CR,0,0,75,75_.jpg"}],

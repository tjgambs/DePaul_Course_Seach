           }
 
        }

		if( typeof A9_SL_CSM_JS != 'undefined' ){
		  A9_SL_CSM_JS($, reformatType);
		}

        function dumpHtml( obj, newAdsHtml, topHtml, bottomHtml, divParent ) {
              var i = 0;
              var completeAdsHtml = '';
              for( x in obj ) {
                  var adChunks = newAdsHtml.split( adPropertiesDelimiter );
                  for( var j = 0; j<adChunks.length; j++ ) {
                      if( typeof obj[i][adChunks[j]] != "undefined" ) {
                          adChunks[j] = obj[i][adChunks[j]];
                      }
                  }
                  completeAdsHtml += adChunks.join("");
                  i++;
              }
              newAdsHtml = topHtml + completeAdsHtml + bottomHtml;
              $('#' + divParent).replaceWith(newAdsHtml);
        }
	});

	P.when('jQuery','ready').execute(function($){
	    var jsonAds = '[{"redirectUrl":"http://rd.a9.com/srv/redirect/?info=AMWTW8l6o0Mn2LMajtLRas16qEPEvhls732xJf5qA0Uwp0mkZxtSEx.a0Sy-pMWsnKcnsqTdvUbdtOupYEUBYo2koDWRlfs7Tk.OJvHLKBe3P47.sXBAgK4jgL2OkgNACjji8mmygqYZLx4yyCKSLnAHhQopl8.Pg8Ucw9Zauc-GvE0wXSlN5zDEiIga9Z9zVJ..Q.dvdIDQ3FTKTC1YaDmwLTcxrvqC-9b.g3QdSpO7eFtUvvcbZDOd5a1GC6-hwnwb.oOQItZehc9nSSUGT3BgvuBaIsC4r9DXIfTPMhqgWb7-ENBcMBwwBoeppYOxVBTgsjUz-qKLHlDdLcvRMVAemhviDq1P1t6644DsOg2FO1q37ampD3956iWt68yBV4GFU4FyNBTn074lC-TN7QgxhwfIUpFq4SCykkgqANZchLvaU8R6GNZfGholTGdIQBvKm5KuYl44bYY5lSv937ecWvPOdq-IqgU0cM0oxUrp7lOxFK75-HWTtv0uO6ga.XFLgdm0pLp621.k.q6ge.q3y8UJHOrDFs.Lc9f4giV07PWOmU4XSP6zZpeoDZwOlYCI9jRBHgvjk1iIlzY6gvCzEBnYh6Sbrm65UHlkH1eCC8R.t0sU8YNP-wjORJ-xpPW6eeCNvcuZStfRqfNTimH-s-j5vqf-Qu7S7ftkIW.TtcMdyUkpWhHdDnUXkkxq7GpidUQtZzQLkfKoUVyqMoXo2vIYHcFmf1.wMu0wXVrKt4NfncE.6QLf3g4OC7CCs4Tm2p8y9n9QPjyie4ZcMDnpoReuti69ZmQ9EyzeIMDjpP0vrxOxbMq.rYzrTlFBhpyuJdT9QqjKQXrw-dqcewU2Yo6dQEHWX6oaoj6lSupY9l5MCWuWmlPY4KLp-TT1VS4hTXPwc6K.yNyvLYFeBEkiQxLL3GVDlBHAza0xid6DYfpeNxGzSS4D9dF6nBNc84kuzowcPkYSLcso7ws2Jd1PgYOQVC8nvDSCCJQysUoqP8QtILK2CsqVUDMzNFd4VAiPsht2uiZGFLEtBjwtv.4GXvPrRB4wYR3kVYOba3hEQoz63FmOeUI31tPjXvrr6oH0xwFyinVERbiBvReYYdGWlMJeBf-BdjqOAkC0h5O-EhgPvE53bHM_&awt=1&s=","visibleUrl":"www.lynda.com/subscribe-today","description":"<b>Design Patterns</b> video tutorials. Watch and learn at your own pace.","provider":"g","title":"<b>Design Patterns</b> Tutorials"}]';
	    var feedbackFormAvailable = 0;
	    function showSLF() {
	         if(!feedbackFormAvailable) {
	           // Make a request to the handler with new ads 
	           $.post('/gp/product/aui_feedback_form_handler.html', { 'sid': '178-0618199-4238264','jsonAds' : escape(jsonAds) } ,function(data) {
	               $('#FeedbackFormDiv').html(data);
	               $('#FeedbackFormDiv').show();
	               $('#ShowFeedbackLinkDiv').hide();
	               $('#ResponseFeedbackLinkDiv').hide(); 
	               feedbackFormAvailable = 1;
	           });
	         } else {
	           $('#reports-ads-abuse-form').show();
	           $('#FeedbackFormDiv').show();
	           $('#reports-ads-abuse').show();
	           $('#ShowFeedbackLinkDiv').hide();
	           $('#ResponseFeedbackLinkDiv').hide(); 
	         }
	    }
	    $('#LetUsKnowLink').click(showSLF);
	    $('#ShowFeedbackLinkDiv').css('opacity','1');
	    $('#ShowFeedbackLinkDiv').css('pointer-events','auto');
	});


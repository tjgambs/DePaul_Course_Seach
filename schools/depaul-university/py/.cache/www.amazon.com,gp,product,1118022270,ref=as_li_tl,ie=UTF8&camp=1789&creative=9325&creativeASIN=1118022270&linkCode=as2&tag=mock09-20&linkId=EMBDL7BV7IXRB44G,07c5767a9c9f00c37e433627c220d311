.aplus .half-col,
.aplus .two-third-col,
.aplus .three-fourth-col,
.aplus .one-col {
    float:left;
    margin-right: 1.6760%;
    overflow: hidden;
}

.aplus .last {
    margin-right:0px;
}

.aplus .sixth-col {
    width: 15.080%;
}
.aplus .fourth-col {
    width: 23.4637%;
}


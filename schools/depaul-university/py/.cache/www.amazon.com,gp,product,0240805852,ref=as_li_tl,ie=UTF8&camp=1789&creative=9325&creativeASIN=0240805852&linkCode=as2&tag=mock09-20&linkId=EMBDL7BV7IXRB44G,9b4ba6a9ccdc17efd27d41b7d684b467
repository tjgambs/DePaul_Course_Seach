	    function showSLF() {
	         if(!feedbackFormAvailable) {
	           // Make a request to the handler with new ads 
	           $.post('/gp/product/aui_feedback_form_handler.html', { 'sid': '189-5919251-4331018','jsonAds' : escape(jsonAds) } ,function(data) {

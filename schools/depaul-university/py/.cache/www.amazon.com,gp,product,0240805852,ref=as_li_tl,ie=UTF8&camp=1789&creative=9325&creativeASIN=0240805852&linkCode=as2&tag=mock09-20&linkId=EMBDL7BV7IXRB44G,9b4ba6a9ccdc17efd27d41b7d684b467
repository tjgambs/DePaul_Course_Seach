        function dumpHtml( obj, newAdsHtml, topHtml, bottomHtml, divParent ) {
              var i = 0;
              var completeAdsHtml = '';
              for( x in obj ) {
                  var adChunks = newAdsHtml.split( adPropertiesDelimiter );
                  for( var j = 0; j<adChunks.length; j++ ) {
                      if( typeof obj[i][adChunks[j]] != "undefined" ) {
                          adChunks[j] = obj[i][adChunks[j]];
                      }
                  }
                  completeAdsHtml += adChunks.join("");
                  i++;
              }
              newAdsHtml = topHtml + completeAdsHtml + bottomHtml;
              $('#' + divParent).replaceWith(newAdsHtml);
        }
	});

	P.when('jQuery','ready').execute(function($){
	    var jsonAds = '[{"visibleUrl":"www.lynda.com/subscribe-today","redirectUrl":"http://rd.a9.com/srv/redirect/?info=AMOjM.2s-IIZd.o4Fse8TYpEATA.WGLFD6L28vm7MRaLsEQU-XeABPsxljLHtKoI-C6Ous9v3i970JvpSibDQSnAAne4T2xiIXCf8OQRpCfjJv4-JV7o0Kre6v1PRAOMI5dXYKb1ITeAtPam1AC-FjR-ee2j6mxNT3qdym7inJ2MEaYR8KMBg4Cg---ae1GbU-eXR.AcKnUgFEvNcURLmfsG4yGHwpuj23W7j9eQB9qZrVXTW4TAihxQELBhQH4OkbKAjeJuBma0dGYD1QOBpqC.y3uS2.1CZleDSotZi9CnAg7MJlDOFlL9QXJoJmzifXVgVC01q28gO6L.wPhQ2cV4s4kvIUHR7s90wMEx6903O.cGYy73jJcw3YMhtz27inAHovdU0XXEYxpfamCavsg-vWA0KsxaEBdribwqp4UYEwtRBo7.ZerQ7gmMdTLW4YnevB4UbMPkjLNiORScVncFW1Gn6EVtarGyfqUscedokWj78J.Svyf5YozVrIFFn0zHsKmS2GjbFEFLNesJ.-Uh1XxBsTNGfY1DKKsgj5Zzf8SdrmqYIgYyVQJSydUBULg5O64zgwnjYdyzrsbNtbxmX4uayv-wYA0UL5b83.4pNntcddkru75c.vepR6gMMN0ItuS-urfEX9udRNW4Pat4BKwLWRSrNyrANJE20cXy2qjdeL5viJL1WToTaXHmEL949l2QfKDWvXm3YiCegPzY1zb9nzR2mlK0mNon7dYq.bK399zDezIEN39H0iqPfHl5KMvgsCUA1TU4wbCANvBIkM3887fZ4FsEnr3qPyEJUxbz7v337a1EmK99MasV-lHv0iSdPk0IkwwQJ9rgUJLXUflxYWBd3kKXM8c1j5lPLZJYg2CIPzBDlIPW12nXPMf8A6d2EHQE8oZA967Wlo3Ffao5DAkrwl901RtqQ64g7EeK5VsEb-xbasFouvZ8yDr3UoGTLZWdCYrbU5UrEQTOritjvZRl4Cw4Am1GuKsZCd2-384IUeHVTXCbDZMbI-S4IxToFzLz9TgXjGD1bbzs3H1Gc4EPTp7w18.oChkjRJAhMNNJb-VtiLD7NbFlrXPwVsSsz-JE&awt=1&s=","description":"<b>Filmmaking</b> online video tutorials. Watch and learn at your own pace.","provider":"g","title":"Learn <b>Filmmaking</b>"}]';

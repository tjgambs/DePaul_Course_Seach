        'litbData' : {
            'hasLitb' : true,
            'litbReftag' : "sib_dp_pt"
        },
        'audibleData' : audibleData,
        'windowWidthThreshold' : 1150,
        'configWidths' : [200, 260],
        'holderRatio' : 0.75,
        'containerMargin' : 15,
        'flipLinkMinHeight' : 0,
        'frontImageWidth' : 231,
        'frontImageHeight' : 346,
        'frontImageAspectRatio' : 0.6676300578034682,
        'imageGalleryData' : [{"mainUrl":"http://ecx.images-amazon.com/images/I/51zQFAzbsOL.jpg","dimensions":[333,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/51zQFAzbsOL._AC_SX75_CR,0,0,75,75_.jpg"},{"mainUrl":"http://ecx.images-amazon.com/images/I/41LseBlpUaL.jpg","dimensions":[375,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/41LseBlpUaL._AC_SX75_CR,0,0,75,75_.jpg"}],
        'centerColMargin' : 40,

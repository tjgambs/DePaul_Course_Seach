        var obj = [{"visibleUrl":"www.bluebeam.com/PDF-Edit","redirectUrl":"http://rd.a9.com/srv/redirect/?info=AIpeYpHugSae3BCw-yz502-56T1U1wSqQ6SIP-bOeJq.k51TfwyxRE6Tw8medj-2QBeBYZWL5W1xPBt4f4VXu7XMGHyFn0CvcrtoMSSaR-P0sbqIRAHERk1vS.iAjxnRL8cytET1vR-ShTyGRvpDKYzlzaYu262CWouTwy6pBnWmV3vU3acQunS7adBDiIGfflN5fj1GpO8z7-bCvyUM1zZvdWb1hTMWQMWEdNPW.tZAWjm2cfoe6dr11n1lTIjWkI.5RLTCWrtG08d1r7-k-IVXV2EyRX-w76y2VjOh1FKoVhCB.mFxjL9FIHAwk4wYjGMZbviueyFR9ZjhofgUCRpt80kS456sC2w4KFcnSLuCyb4-.FxWUJtGrRUn0t-BAj0OodiFPTLJZnmm0-.751e3I9UW1GtcNV8UDROf3KdpNJKGOMC1X45TZmsFXdKT3bmV4krooKBWDOPpSNO7Jca72LLvSJ7Wd.1RGpx18IoQjO1E9cq.OKQ5DfYmSjRPpDH9nRJsxVO52xF2FItosSurlMhF-Xv9Xv0Ob3-um3QHUvaB191Sz-VJAVn8snviACjokrLAMRYhm45MoJ3bHoL1zHE0E2LiZwyiOnMrGa00YajJb.-DiA9-8q1E6EnoBWZaltnnXKi1TGf0WfazUCTtTgnKDmAmNXbRvwKeCQ8yO9fO693qYK8IC6fO80uzkkMR-RMbbzISNdFUttS3NupyPZ51gF52EK.wwhrTN5.4&awt=1&s=","description":"Work Smarter Than Ever Before. Get Free Trial Of Bluebeam® Revu®","provider":"g","title":"<b>Linux</b> PDF <b>Editor</b>"}];
        var adPropertiesDelimiter = "__";
        var adCount = 1;
        var slDiv =  'AUI_SlDiv';
        var expectedHeightWide = 34;
        var expectedHeightMiddle = 56;
       	var reformatType = '0';
       	

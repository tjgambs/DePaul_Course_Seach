        function dumpHtml( obj, newAdsHtml, topHtml, bottomHtml, divParent ) {
              var i = 0;
              var completeAdsHtml = '';
              for( x in obj ) {
                  var adChunks = newAdsHtml.split( adPropertiesDelimiter );
                  for( var j = 0; j<adChunks.length; j++ ) {
                      if( typeof obj[i][adChunks[j]] != "undefined" ) {
                          adChunks[j] = obj[i][adChunks[j]];
                      }
                  }
                  completeAdsHtml += adChunks.join("");
                  i++;
              }
              newAdsHtml = topHtml + completeAdsHtml + bottomHtml;
              $('#' + divParent).replaceWith(newAdsHtml);
        }
	});

	/*** Client Side Metrics Code ***/

	//use global variable to pass the parameter
	var a9_sl_reformatType;
	function A9_SL_CSM_JS ($, reformatType) {
		a9_sl_reformatType = reformatType;
	
			//wait for cf signal
		   P.when('jQuery', 'cf').execute(function($) {
			var lantencyfix=true;
			var appeared = false;
			var widgetId = "AUI_A9AdsMiddleBoxTop";
			var tableClass = "AUI_SponsoredLinkColumnAds";
			
			var viewSelector = ".AUI_SponsoredLinkColumnAds tr:eq(0)";
			if ( reformatType === '1' ){
				viewSelector = ".AUI_SponsoredLinkColumnAds tr:eq(1)";
			} else if ( reformatType === '2' ){
				viewSelector = ".AUI_SponsoredLinkItem:eq(0)";
			}
			
			var requestTag = "AUPluzySKS7UU1t0";
			var samplingType = "0";
			var sessionId = "191-7311575-6589931";
			var marketplace = "US";
			
			var hoverTimeEnter = 0; // milliseconds
			var hoverTimeDef = 1000;  // milliseconds
			
			var fVip = "https://fls-na.amazon.com";
			var fApiVersion = "1";
			var fChannelId = "a9-sl";
			var fChannelVersion = "1";
			var fAction = "OP";
			var foresterUrlBase = [fVip, fApiVersion, fChannelId, fChannelVersion, fAction, "?"].join("/");
			
			function sendToForester(queryParams) {
				var repeatFlag = 0;
				
				// Back and forward button detection (if information is present)
				var bfDetect;
		       	if (document.ue_backdetect && document.ue_backdetect.ue_back) {
		         	bfDetect = document.ue_backdetect.ue_back;
		           	if (bfDetect.value > 2) {
		            	repeatFlag = 1;
		           	}
		       	}
				
				if (repeatFlag === 0) {
					var queryString = "";
					for (key in queryParams) {
						queryString += key + "=" + encodeURIComponent(queryParams[key]) + "&";
					}
					new Image().src = foresterUrlBase 
										+ queryString 
										+ "marketplace=" + encodeURIComponent(marketplace)
										+ "&sessionId=" + encodeURIComponent(sessionId)
										+ "&samplingType=" + encodeURIComponent(samplingType)
										+ "&requestTag=" + encodeURIComponent(requestTag);
				}
			}
		
			function elementIsViewable (selector) {
				return ( elementHasHeightAndWidth(selector) && isScrolledIntoView(selector) && $(selector).is(":visible") );
			}
		
			function isScrolledIntoView(elem) {
			    var docViewTop = $(window).scrollTop();
			    var docViewBottom = docViewTop + $(window).height();
	
			    var elemTop = $(elem).offset().top;
			    var elemBottom = elemTop + $(elem).height();
	
			    return ((elemBottom <= docViewBottom) && (elemTop >= docViewTop));
			}
			
			
			//using amazon javascript 	
			amznJQ.onReady('callOnVisible',function(){
				if (typeof $('#'+widgetId).callOnVisible == 'function') {
				
					//calculate hight for accruate recording
					var height = $('#'+widgetId).height();
					$('#'+widgetId).callOnVisible(function (){
						if (appeared==false){
							appeared = true;
						
							var params = {};
							params["eventType"] = "1";
							params["widgetScrollView"] = "1";
						
							sendToForester(params);
						}
					}, -height);
				}	 
			});
			
			function elementHasHeightAndWidth (elem) {
				var height = $(elem).height();
				var width = $(elem).width();
				
				return (height > 0 && width > 0);
			}
			
		
			var pageLoadedParams = {};
			pageLoadedParams["eventType"] = "0";
			pageLoadedParams["reformatType"] = a9_sl_reformatType;
			
			// Ad-blocking detection
			/*
				Note: The definition of visibility differs between jQuery 1.3.1 (and older) and jQuery 1.3.2+
			*/
			if ( $("#"+widgetId).is(':hidden') ||  !elementHasHeightAndWidth("#"+widgetId) ) {
				pageLoadedParams["adblockOn"] = "1";
			} else {
				pageLoadedParams["adblockOn"] = "0";
			}	
			
			// check for appearance upon load even if no scrolling/resizing happens
			if ( appeared === false && elementIsViewable(viewSelector) ) {
				appeared = true;
				pageLoadedParams["widgetInitView"] = "1";	
			} else {
				pageLoadedParams["widgetInitView"] = "0";
			}
			
			// Browser dimensions
			pageLoadedParams["browserDimX"] = $(window).width(); 
			pageLoadedParams["browserDimY"] = $(window).height();
			
			// Ads location
			var adsLocX = $("#"+widgetId).offset().left;
			var adsLocY = $("#"+widgetId).offset().top;
		
		    // we noticed there are some abnormal value logged. So we try to filter the value which is out of the range we set
			if ( !adsLocX || adsLocX > 2560 || adsLocX < 0 )
			{
			     adsLocX = -1;
			}
			if ( !adsLocY || adsLocY > 10000 || adsLocY < 0 )
			{
			     adsLocY = -1;
			}
					
			pageLoadedParams["adsLocX"] = adsLocX;
			pageLoadedParams["adsLocY"] = adsLocY;			
		
			
			// Screen resolution
			pageLoadedParams["screenResX"] = screen.width;
			pageLoadedParams["screenResY"] = screen.height;
			
			//Page Depth
			pageLoadedParams["pageDepth"] = $(document).height();
			
			//--- SEND PAGE LOAD EVENT DATA ---
			sendToForester(pageLoadedParams);
			
			
			// hover bind
			
			$("#"+widgetId).hover(
				function () {
					hoverTimeEnter = new Date().getTime();
				},
				function () {
					var hoverTimeExit = new Date().getTime();
					if ( (hoverTimeExit - hoverTimeEnter) >= hoverTimeDef ){
						$("#"+widgetId).unbind("mouseenter mouseleave");
	
					 	var params = {};
						params["eventType"] = "2";
						params["hoverOnWidget"] = "1";
	
						sendToForester(params);
					}
				}
			);
		


	});
	
}

	P.when('jQuery','ready').execute(function($){
	    var jsonAds = '[{"visibleUrl":"www.bluebeam.com/PDF-Edit","redirectUrl":"http://rd.a9.com/srv/redirect/?info=AIpeYpHugSae3BCw-yz502-56T1U1wSqQ6SIP-bOeJq.k51TfwyxRE6Tw8medj-2QBeBYZWL5W1xPBt4f4VXu7XMGHyFn0CvcrtoMSSaR-P0sbqIRAHERk1vS.iAjxnRL8cytET1vR-ShTyGRvpDKYzlzaYu262CWouTwy6pBnWmV3vU3acQunS7adBDiIGfflN5fj1GpO8z7-bCvyUM1zZvdWb1hTMWQMWEdNPW.tZAWjm2cfoe6dr11n1lTIjWkI.5RLTCWrtG08d1r7-k-IVXV2EyRX-w76y2VjOh1FKoVhCB.mFxjL9FIHAwk4wYjGMZbviueyFR9ZjhofgUCRpt80kS456sC2w4KFcnSLuCyb4-.FxWUJtGrRUn0t-BAj0OodiFPTLJZnmm0-.751e3I9UW1GtcNV8UDROf3KdpNJKGOMC1X45TZmsFXdKT3bmV4krooKBWDOPpSNO7Jca72LLvSJ7Wd.1RGpx18IoQjO1E9cq.OKQ5DfYmSjRPpDH9nRJsxVO52xF2FItosSurlMhF-Xv9Xv0Ob3-um3QHUvaB191Sz-VJAVn8snviACjokrLAMRYhm45MoJ3bHoL1zHE0E2LiZwyiOnMrGa00YajJb.-DiA9-8q1E6EnoBWZaltnnXKi1TGf0WfazUCTtTgnKDmAmNXbRvwKeCQ8yO9fO693qYK8IC6fO80uzkkMR-RMbbzISNdFUttS3NupyPZ51gF52EK.wwhrTN5.4&awt=1&s=","description":"Work Smarter Than Ever Before. Get Free Trial Of Bluebeam® Revu®","provider":"g","title":"<b>Linux</b> PDF <b>Editor</b>"}]';
	    var feedbackFormAvailable = 0;
	    function showSLF() {
	         if(!feedbackFormAvailable) {
	           // Make a request to the handler with new ads 
	           $.post('/gp/product/aui_feedback_form_handler.html', { 'sid': '191-7311575-6589931','jsonAds' : escape(jsonAds) } ,function(data) {
	               $('#FeedbackFormDiv').html(data);
	               $('#FeedbackFormDiv').show();
	               $('#ShowFeedbackLinkDiv').hide();
	               $('#ResponseFeedbackLinkDiv').hide(); 
	               feedbackFormAvailable = 1;
	           });
	         } else {
	           $('#reports-ads-abuse-form').show();
	           $('#FeedbackFormDiv').show();
	           $('#reports-ads-abuse').show();
	           $('#ShowFeedbackLinkDiv').hide();
	           $('#ResponseFeedbackLinkDiv').hide(); 
	         }
	    }
	    $('#LetUsKnowLink').click(showSLF);
	    $('#ShowFeedbackLinkDiv').css('opacity','1');
	    $('#ShowFeedbackLinkDiv').css('pointer-events','auto');
	});

</script>



 






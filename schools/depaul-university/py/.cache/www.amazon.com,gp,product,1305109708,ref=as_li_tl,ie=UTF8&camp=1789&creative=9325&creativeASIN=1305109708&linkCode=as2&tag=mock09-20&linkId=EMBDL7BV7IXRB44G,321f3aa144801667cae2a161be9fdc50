P.when("A", "ready").execute(function(A){
  var triggered = 0;
  var reviewsSection = A.$("#cm_cr_dpwidget");  
  onScreenTriggerMethod();
  A.on.scroll(onScreenTriggerMethod);
  
  function onScreenTriggerMethod(){
    if (!triggered && A.onScreen(reviewsSection)) {
      triggered = 1;
      A.post('/gp/customer-reviews/aj/customer/ranking-trigger', {
          success: function(data){
          },
 
          error: function(data){
          },

          params: {
            csrfT: '0.023817560672434;1449018477;28d25d12fea749ca3270ebf94b17f4dab1527467abf2b85f1f1d9754f832bdb3',
            requestId: '0JEGPCA1QTNBPK3ANZYN',
            sessionId: '179-9118396-8060655'
          }
      });
    }
  }

});

</script>









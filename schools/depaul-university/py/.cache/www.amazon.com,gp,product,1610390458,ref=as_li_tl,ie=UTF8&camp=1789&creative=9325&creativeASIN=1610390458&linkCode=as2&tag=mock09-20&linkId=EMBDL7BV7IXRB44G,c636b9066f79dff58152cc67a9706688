<script type="text/javascript">
  window.$Nav && $Nav.when('$').run('INMarketplaceRedirectOverlay',function($) {
    $.ajax({
      type: 'POST',
      url: '/gp/redirection/india.html',
      data: {
        path: '/gp/product/1610390458',
        queryString: '?ie=UTF8\u0026linkId=EMBDL7BV7IXRB44G\u0026redirect=true\u0026tag=mock09-20\u0026linkCode=as2\u0026camp=1789\u0026creative=9325\u0026creativeASIN=1610390458',
        pageType: 'Detail',
        referer: ''
      },
      success: function(data) {
        if (data) {
          $('body').append(data);
        }
      }
    });
  });
</script>

    








  





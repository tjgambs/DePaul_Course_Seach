    var audibleData = {};
    if(false){
        audibleData = {
            'audioSampleSrc' : "",
            'flashPlayerUrl' : ""
        }
    }
    var data = {
        
        'litbData' : {
            'hasLitb' : false,
            'litbReftag' : "sib_dp_pt"
        },
        'audibleData' : audibleData,
        'windowWidthThreshold' : 1150,
        'configWidths' : [200, 260],
        'holderRatio' : 0.75,
        'containerMargin' : 15,
        'flipLinkMinHeight' : 0,
        'frontImageWidth' : 260,
        'frontImageHeight' : 227,
        'frontImageAspectRatio' : 1.145374449339207,
        'imageGalleryData' : [{"mainUrl":"http://ecx.images-amazon.com/images/I/51yGZQrMJuL.jpg","dimensions":[500,436],"thumbUrl":"http://ecx.images-amazon.com/images/I/51yGZQrMJuL._AC_SY75_CR,0,0,75,75_.jpg"}],
        'centerColMargin' : 40,
        'disableResize' : false
    };
    return data;
});

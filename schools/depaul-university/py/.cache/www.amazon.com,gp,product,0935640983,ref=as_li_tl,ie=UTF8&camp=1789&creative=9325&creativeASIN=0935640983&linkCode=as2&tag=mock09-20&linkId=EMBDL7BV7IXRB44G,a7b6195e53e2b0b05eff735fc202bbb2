  P.when("jQuery").execute(function(jQuery) {
      P.when("reviewsLightbox-js").execute(function() {
      
       
       var reviewImageGallery = new ReviewGallery("R2CIKGRADQB26U_gallerySection_DD7DE8AA-988C-11E5-B479-CD71386BB31B", "DESKTOP");

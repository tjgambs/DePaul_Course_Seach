            'hasLitb' : true,
            'litbReftag' : "sib_dp_pt"
        },
        'audibleData' : audibleData,
        'windowWidthThreshold' : 1150,
        'configWidths' : [200, 260],
        'holderRatio' : 0.75,
        'containerMargin' : 15,
        'flipLinkMinHeight' : 25,
        'frontImageWidth' : 221,
        'frontImageHeight' : 346,
        'frontImageAspectRatio' : 0.638728323699422,
        'imageGalleryData' : [{"mainUrl":"http://ecx.images-amazon.com/images/I/5195R1wCSLL.jpg","dimensions":[319,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/5195R1wCSLL._AC_SX75_CR,0,0,75,75_.jpg"},{"mainUrl":"http://ecx.images-amazon.com/images/I/510Wx8OPp-L.jpg","dimensions":[330,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/510Wx8OPp-L._AC_SX75_CR,0,0,75,75_.jpg"},{"mainUrl":"http://ecx.images-amazon.com/images/I/31J9MdAf3CL.jpg","dimensions":[375,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/31J9MdAf3CL._AC_SX75_CR,0,0,75,75_.jpg"}],

<hr noshade="true" size="1" class="bucketDivider"/>

<div id="pd-available"></div>






<script type="text/javascript">
var ProductDescriptionIframeResize = {};
(function () {
  var d = document;
  var obj = this;
  var nTries = 1;
  var hasCustomWidth = 0;
  var MAX_TRIES = 20;
  obj.iframeId = 'product-description-iframe';
  obj.appendTo = 'iframe-wrapper';
  function getDocHeight (doc) {
    var contentDiv = doc.getElementById("pdIframeContent");
    var docHeight = 0;
    if(contentDiv){
      docHeight = Math.max(
        contentDiv.scrollHeight,
        contentDiv.offsetHeight,
        contentDiv.clientHeight
      );
    }
    return docHeight;
  }  
  function isElementOnRight(pdRef, el) {
    var ret=0;
    try {
      if(pdRef, el) {
        if((el.offsetTop < pdRef.offsetTop) && (el.offsetTop + el.offsetHeight > pdRef.offsetTop)) {
          ret = 1;
        }
      }
    } catch (e) {}
    return ret;
  }
  function setWidth(iframeRef, el) {
    var elLeft, w, minW = 450;
    try {
      elLeft = el.offsetLeft;
      w = elLeft - 50;
      if(w > minW) {
        iframeRef.style.width = w + 'px';
        hasCustomWidth = 1;
      }
    } catch (e) {}
  }
  function setIframeWidth(ifrmId) {
    var iframeRef = d.getElementById(ifrmId),
        pdRef = d.getElementById("pd-available"),
        rightCol = d.getElementById("rightCol"),
        rightRail = d.getElementById("rightRail");
    if(isElementOnRight(pdRef, rightRail)) {
      setWidth(iframeRef, rightRail); 
    } else if (isElementOnRight(pdRef, rightCol)) {
      setWidth(iframeRef, rightCol);
    }
  }
  function setIframeHeight (ifrmId) {
    var iframeDoc, iframeRef = d.getElementById(ifrmId);
    try {
      iframeDoc = iframeRef.contentWindow.document;
    } catch (e) {
      try {
        iframeDoc = iframeRef.contentDocument;
      } catch (ex) {}
    }
    if (iframeDoc) {
      var h = getDocHeight(iframeDoc);
      if (h && h != 0) {
        iframeRef.style.height = h + 'px';
      } else { 
        if (nTries < MAX_TRIES) {
          nTries++;
          setTimeout(function () {
            setIframeHeight(ifrmId);
          }, 50); 
        }
      }
    }
  }
  obj.iframeload = function () {
    var iframe = d.getElementById(obj.iframeId);
    iframe.style.display = '';
    iframe.style.height = "1px";
    setTimeout(function () {
      if(obj.needWidthAdjust) {
        setIframeWidth(obj.iframeId);
      }
      setIframeHeight(obj.iframeId);
      var onloadCallback = obj.onloadCallback;
      if(typeof onloadCallback === "function") {
        onloadCallback();
      }
    }, 50); 

  }
  obj.createIframe = function ( iframeId, onloadFn ) {
    var iframe = d.createElement('iframe');
    iframe.id = iframeId;
    iframe.className = "ap_never_hide";
    iframe.width = "100%";
    iframe.scrolling="no";
    iframe.frameBorder="0";
    if ( iframe.addEventListener )
      iframe.addEventListener("load", onloadFn, false);
    else if ( iframe.attachEvent )
      iframe.attachEvent("onload", onloadFn);
    else iframe.onload = onloadFn;
    return iframe; 
  }
  obj.initialize = function (onloadCallback, needWidthAdjust) {
    var iframeContent = "%0A%20%20%0A%0A%0A%0A%3Chtml%3E%0A%20%20%3Chead%3E%0A%20%20%20%20%3Cstyle%20type%3D'text%2Fcss'%3E%0A%0A%0A%20h2%20%7B%0A%20%20%20%20%20%20color%3A%23333%20!important%3B%0A%20%20%20%20%20%20font-size%3A21px%20!important%3B%0A%20%20%20%20%20%20line-height%3A%201.3%3B%0A%20%20%20%20%20%20padding-bottom%3A%204px%3B%0A%20%20%20%20%20%20font-weight%3A%20normal%3B%0A%20%20%20%20%20%20font-family%3AArial%2C%20sans-serif%3B%0A%20%20%20%20%20%20margin%3A%200px%3B%0A%20%7D%0A%20%20%20%20%0Abody%20%7B%0A%20%20background-color%3A%20%23FFFFFF%3B%0A%20%20margin%3A%200px%3B%0A%20%20height%3A%20100%25%3B%0A%20%20width%3A%20100%25%3B%0A%20%20color%3A%20%23333333%3B%0A%20%20word-wrap%3A%20break-word%3B%20%0A%20%20font-family%3A%20verdana%2Carial%2Chelvetica%2Csans-serif%3B%0A%20%20font-size%3A%20small%3B%0A%7D%0A%0Atd%2C%20th%20%7B%0A%20%20font-family%3A%20verdana%2Carial%2Chelvetica%2Csans-serif%3B%0A%20%20font-size%3A%20small%3B%0A%7D%0A%0Ahr%20%7B%0A%20%20border-top%3A%201px%20dashed%20%23999999%3B%0A%20%20height%3A%201px%3B%0A%20%20color%3A%20%23FFFFFF%3B%0A%20%20margin%3A%203px%200px%3B%0A%20%20border-style%3A%20dashed%20none%20none%20none%3B%0A%20%20background-color%3A%20white%0A%7D%0Aul%20%7B%20%0A%20%20list-style-type%3A%20none%3B%0A%20%20margin%3A%200px%3B%20%0A%20%20padding%3A%200px%3B%20%0A%7D%0Aul%20li%20%7B%0A%20%20margin%3A%200.5em%200em%3B%0A%7D%0Aul%20li%20ul%20%7B%0A%20%20list-style-type%3A%20none%3B%0A%20%20margin-left%3A%2025px%3B%0A%7D%0Aul%20li%20ul%20li%20%7B%0A%20%20margin%3A%200em%3B%0A%7D%0Adiv.bucket%20%7B%20padding%3A%205px%200em%3B%20%7D%0Adiv.bucket%20div.content%20%7B%20margin%3A%200.5em%200px%200em%2025px%3B%20%7D%0Ahr%20%7B%20margin-left%3A%200px%3B%20%7D%0A.bucket%20h3%20%7B%0A%20%20color%3A%20%23000%3B%0A%20%20font-size%3A%201em%3B%0A%20%20font-weight%3A%20bold%3B%0A%20%20margin%3A%200px%200px%200.25em%200px%3B%0A%7D%0Ahr.bucketDivider%20%7B%20clear%3A%20left%3B%20%7D%0Adiv.bucket%20%7B%20padding%3A%205px%200em%3B%20%7D%0A.disclaim%20%7B%0A%20%20margin-top%3A%203px%3B%0A%20%20font-size%3A%2010px%3B%0A%20%20color%3A%20%23888%3B%0A%7D%0Abody%20hr%20%7B%0A%20%20border-top%3A%201px%20solid%20%23CCCCCC%3B%0A%7D%0Aa%3Alink%20%7B%0A%20%20color%3A%20%23003399%3B%0A%20%20text-decoration%3A%20underline%3B%0A%7D%0Aa%3Ahover%20%7B%0A%20%20color%3A%20%23CC6600%3B%0A%20%20text-decoration%3A%20none%3B%0A%7D%0Aa%3Aactive%20%7B%0A%20%20color%3A%20%23FF9933%3B%0A%7D%0Aa%3Avisited%20%7B%0A%20%20color%3A%20%23996633%3B%0A%7D%0A%0A%0A%0A%23productDescription%20h2.productDescriptionHeader%20%7B%0A%20%20%20%20margin-bottom%3A%200em%3B%09%0A%7D%0A%0A%23productDescription%20.emptyClear%20%7B%0A%20%20%20%20clear%3Aleft%3B%0A%20%20%20%20height%3A0px%3B%0A%20%20%20%20font-size%3A0px%3B%0A%7D%0A%0A%23productDescription%20div.productDescriptionWrapper%20%7B%0A%20%20%20%20margin%3A%200%200%201em%200%3B%09%09%0A%7D%0A%0A%23productDescription%20h3.productDescriptionSource%20%7B%0A%20%20%20%20font-weight%3Anormal%3B%0A%20%20%20%20color%3A%23333333%3B%0A%20%20%20%20font-size%3A1.23em%3B%0A%20%20%20%20margin%3A%20.75em%200%20.375em%20-15px%3B%0A%20%20%20%20clear%3Aleft%3B%0A%7D%0A%0A%23productDescription%20.seeAll%20%7B%0A%09margin-top%3A%201.25em%3B%20%0A%09margin-left%3A%20-15px%3B%20%0A%7D%0A%0A%23productDescription%20ul%2C%20%23technicalProductFeatures%20ul%20%7B%20%0A%20%20list-style-type%3A%20disc%3B%20%0A%20%20margin%3A%201.12em%200%3B%20%0A%20%20margin-left%3A%2020px%3B%20%0A%7D%0A%0A%23productDescription%20ul%20li%20%7B%20%0A%20%20margin%3A%200%200%200%2020px%3B%20%0A%7D%0A%0A%23productDescription%20ul%20li%20ul%20%7B%20%0A%20%20list-style-type%3A%20disc%3B%20%0A%20%20margin-left%3A%2020px%3B%20%0A%7D%0A%0A%23productDescription%20ul%20li%20ul%20li%20%7B%20%0A%20%20margin%3A%200%200%200%2020px%3B%20%0A%7D%0A%0A%23productDescription%20.aplus%20h4%2C%20%23productDescription%20.aplus%20h5%20%7B%0A%20%20%20%20margin%3A%200%200%20.75em%200%3B%0A%20%20%20%20font-size%3A%201em%3B%0A%7D%0A%0A%23productDescription%20.aplus%20h4%20%7B%0A%20%20%20%20color%3A%20%23CC6600%3B%0A%7D%0A%0A%23productDescription%20.aplus%20p%20%7B%0A%20%20%20%20margin%3A%200%200%201em%200%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.break%20%7B%0A%20%20%20%20clear%3Aboth%3B%0A%20%20%20%20height%3A0px%3B%0A%20%20%20%20font-size%3A0px%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.spacer%20%7B%0A%20%20%20%20margin-bottom%3A%2013px%3B%0A%7D%0A%0A%23productDescription%20.aplus%20img%20%7B%0A%20%20%20%20border%3Anone%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.leftImage%2C%20%23productDescription%20.aplus%20.rightImage%2C%20%23productDescription%20.aplus%20.centerImage%20%7B%0A%20%20%20%20margin-bottom%3A%201em%3B%0A%20%20%20%20margin-top%3A%200%3B%0A%20%20%20%20text-align%3Acenter%3B%0A%20%20%20%20vertical-align%3Atop%3B%0A%7D%0A%0A%0A%23productDescription%20.aplus%20.leftImage%20%7B%0A%20%20%20%20margin-right%3A%2015px%3B%0A%20%20%20%20float%3Aleft%3B%0A%20%20%20%20clear%3Aleft%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.rightImage%20%7B%0A%20%20%20%20margin-left%3A%2015px%3B%0A%20%20%20%20float%3Aright%3B%0A%20%20%20%20clear%3Aright%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.imageCaption%20%7B%0A%20%20%20%20clear%3Aboth%3B%0A%20%20%20%20padding%3A%20.5em%20.5em%200%20.5em%3B%0A%20%20%20%20font-size%3A%20.846em%3B%0A%20%20%20%20display%3A%20block%3B%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20%7B%20%0A%09border-collapse%3A%20collapse%3B%20%0A%09margin-bottom%3A%201.25em%3B%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20th%20%7B%20%0A%09font-weight%3A%20bold%3B%20%0A%09background%3A%20%23F7F7F7%3B%20%0A%09border-style%3Asolid%3B%20%0A%09border-color%3A%20%23CCCCCC%3B%20%0A%09border-width%3A0%200%201px%201px%3B%20%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20td%20%7B%20%0A%09border-left%3A%201px%20solid%20%23CCC%3B%20%0A%09border-bottom%3A%201px%20dotted%20%23CCC%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20th%2C%20%23productDescription%20.aplus%20table.data%20td%0A%7B%20%0A%09padding%3A3px%2010px%3B%20%0A%09text-align%3Aleft%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20tfoot%20%7B%20%0A%09font-style%3A%20italic%3B%20%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20caption%20%7B%0A%09background%3A%20%23eee%3B%20%0A%09font-size%3A%20.8125em%3B%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20tr%20td%3Afirst-child%2C%20%23productDescription%20.aplus%20table.data%20tr%20th%3Afirst-child%20%7B%0A%09border-left-width%3A0px%3B%0A%7D%0A%0A%23productDescription%20.aplus%20ul%20%7B%0A%09margin%3A0%200%201em%200%3B%0A%7D%0A%0A%0A%23productDescription%20.aplus%20.center%20%7B%0A%09text-align%3A%20center%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.right%20%7B%0A%09text-align%3A%20right%3B%0A%7D%0A%0A%23productDescription%20.aplus%20%20.sixth-col%2C%0A%23productDescription%20.aplus%20.fourth-col%2C%0A%23productDescription%20.aplus%20.third-col%2C%0A%23productDescription%20.aplus%20.half-col%2C%0A%23productDescription%20.aplus%20.two-third-col%2C%0A%23productDescription%20.aplus%20.three-fourth-col%2C%0A%23productDescription%20.aplus%20.one-col%20%7B%0A%20%20%20%20float%3Aleft%3B%0A%20%20%20%20margin-right%3A%201.6760%25%3B%0A%20%20%20%20overflow%3A%20hidden%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.last%20%7B%0A%20%20%20%20margin-right%3A0px%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.sixth-col%20%7B%0A%20%20%20%20width%3A%2015.080%25%3B%0A%7D%0A%23productDescription%20.aplus%20.fourth-col%20%7B%0A%20%20%20%20width%3A%2023.4637%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.third-col%20%7B%0A%20%20%20%20width%3A%2031.8436%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.half-col%20%7B%0A%20%20%20%20width%3A%2048.6034%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.two-third-col%20%7B%0A%20%20%20%20width%3A%2065.3631%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.three-fourth-col%20%7B%0A%20%20%20%20width%3A%2073.7430%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.one-col%20%7B%0A%20%20%20%20width%3A%2098.8827%25%3B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.last%20%7B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20100%25%3B%0A%20%20%20%20min-width%3A%20895px%3B%0A%7D%0A%0A*%20html%20%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20expression((document.body.clientWidth%20%3C%20936)%20%3F%20%22895px%22%20%3A%20%22100%25%22%20)%3B%0A%7D%0A%0A%20%20%20%20%3C%2Fstyle%3E%0A%20%20%20%20%3Cbase%20target%3D'_top'%20%2F%3E%0A%20%20%3C%2Fhead%3E%0A%20%20%3Cbody%3E%0A%20%20%3Cdiv%20id%3D%22pdIframeContent%22%3E%0A%0A%0A%0A%0A%20%0A%0A%0A%0A%0A%3Cdiv%20id%3D%22biss-product-description-and-details%22%3E%3C%2Fdiv%3E%0A%0A%3Cdiv%20class%3D%22bucket%22%20id%3D%22productDescription%22%3E%0A%20%20%3Ch2%3EEditorial%20Reviews%3C%2Fh2%3E%0A%0A%3Cdiv%20class%3D%22content%22%3E%0A%0A%0A%20%20%20%20%0A%20%20%20%20%20%20%20%3Ch3%20class%3D%22productDescriptionSource%22%3EReview%3C%2Fh3%3E%0A%20%20%20%20%20%20%20%3Cdiv%20class%3D%22productDescriptionWrapper%22%3E%0A%20%20%20%20%20%20%20%22Great%20coverage%20of%20topics%2C%20supporting%20research%2C%20and%20helpful%20examples%2Fscenarios.%22%3Cbr%20%2F%3E%3Cbr%20%2F%3E%22The%20text%20provides%20the%20teacher%20the%20perspective%20of%20a%20parent%20and%20the%20importance%20of%20respecting%20the%20elements%20and%20indicators%20of%20developmentally%20appropriate%20practices%20to%20meet%20the%20goal%20for%20the%20child%2C%20not%20personal%20bias.%20Teachers%20are%20challenged%20to%20work%20with%20parents%20who%20come%20from%20hostile%20to%20undernurtured%20home%20settings%20or%20lack%20positive%20interpersonal%20skills.%20The%20text%20directs%20the%20readers'%20attention%20to%20not%20only%20developmentally%20appropriate%20practices%20according%20to%20the%20NAEYC%20code%20of%20ethics%2C%20but%20also%20to%20websites%20to%20link%20different%20resources%20related%20to%20the%20subject%20matter.%22%0A%20%20%20%20%20%20%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%0A%20%20%20%20%0A%20%20%20%20%20%20%20%3Ch3%20class%3D%22productDescriptionSource%22%3EAbout%20the%20Author%3C%2Fh3%3E%0A%20%20%20%20%20%20%20%3Cdiv%20class%3D%22productDescriptionWrapper%22%3E%0A%20%20%20%20%20%20%20Carol%20Gestwicki%20was%20an%20instructor%20in%20the%20early%20childhood%20education%20program%20at%20Central%20Piedmont%20Community%20College%20in%20Charlotte%2C%20North%20Carolina%2C%20for%20over%2030%20years.%20Her%20teaching%20responsibilities%20have%20included%20supervising%20students%20in%20classroom%20situations%20as%20they%20work%20with%20families.%20Earlier%20in%20her%20career%2C%20she%20worked%20with%20children%20and%20families%20in%20a%20variety%20of%20community%20agencies%20and%20schools%20in%20Toronto%2C%20New%20York%2C%20New%20Jersey%2C%20and%20Namibia%20(South%20West%20Africa).%20%20She%20received%20her%20MA%20from%20Drew%20University.%20She%20has%20been%20an%20active%20member%20of%20the%20NAEYC%20for%20many%20years%2C%20including%20making%20numerous%20presentations%20at%20state%20and%20national%20conferences.%20She%20has%20been%20a%20Fellow%20in%20the%20Early%20Childhood%20Leadership%20Development%20Project%20at%20the%20University%20of%20North%20Carolina%20at%20Chapel%20Hill%2C%20and%20she%20has%20been%20associated%20with%20the%20T.E.A.C.H.%20Model%2FMentor%20program.%20%20Her%20other%20publications%20include%20more%20than%20two%20dozen%20articles%20about%20child%20development%20and%20family%20issues%20and%20scripts%20and%20design%20for%2014%20audiovisual%20instructional%20programs.%20She%20has%20three%20other%20books%20on%20topics%20in%20early%20education%20published%20by%20Delmar%20Learning%3A%20Developmentally%20Appropriate%20Practice%3A%20Curriculum%20and%20Development%20in%20Early%20Education%2C%20Fifth%20edition%20(2013)%3B%20Essentials%20of%20Early%20Education%20(1997)%3B%20and%20Authentic%20Childhood%3A%20Exploring%20Reggio%20Emilia%20in%20the%20Classroom%20(2002).%20Currently%2C%20she%20%20writes%20a%20regular%20column%20titled%20%22Grandma%20Says%22%20for%20Growing%20Child.%0A%20%20%20%20%20%20%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fdiv%3E%0A%3C%2Fdiv%3E%0A%0A%0A%0A%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fbody%3E%0A%3C%2Fhtml%3E%0A";
    obj.onloadCallback = onloadCallback;
    obj.needWidthAdjust = needWidthAdjust;
    var iframe = obj.createIframe (obj.iframeId, obj.iframeload);
    var wrapper = d.getElementById(obj.appendTo);
    wrapper.innerHTML = '';

    var anchor = d.createElement('div');
    anchor.id = 'biss-product-description-and-details';
    wrapper.appendChild(anchor);

    iframe = wrapper.appendChild(iframe);
    var doc;
    try {
      doc = iframe.contentWindow.document;
    } catch (e) {
      try {
        doc = iframe.contentDocument;
      } catch (ex) {}
    }
    if (doc && doc.open) {
      obj.doc = doc;
      doc.open().write(decodeURIComponent(iframeContent));
      doc.close();
    }
  }
  obj.resizeIframe = function() {
    if(hasCustomWidth) {
      setIframeWidth(obj.iframeId);
    }
    nTries = 1;
    setIframeHeight(obj.iframeId);
  }
}).apply(ProductDescriptionIframeResize);

amznJQ.onCompletion('amznJQ.criticalFeature', function () {
  amznJQ.available('jQuery',function(){
    setTimeout(function() {
      var iframeLoadFn = function() {
        var $ = jQuery;
        $("#pd-available").trigger("pdAvailable");
        $(window).resize(ProductDescriptionIframeResize.resizeIframe);
      };
      ProductDescriptionIframeResize.initialize(iframeLoadFn, 1);
    }, 50);
  });
});
</script>




	    function showSLF() {
	         if(!feedbackFormAvailable) {
	           // Make a request to the handler with new ads 
	           $.post('/gp/product/aui_feedback_form_handler.html', { 'sid': '191-8823837-3085302','jsonAds' : escape(jsonAds) } ,function(data) {

        <script type="text/javascript">
            P.now("LUXContext").execute(function(LUXContext) {
                if (!LUXContext) {
                    P.declare("LUXContext", {
                        isRecognizedCustomer: false,
                        deviceType: "web",
                        isOneClick: false,
                        clientPage: "DPX"
                    });
                }
            });

            P.when("LUXController").execute(function(LUXController) {
                LUXController.init({
                    addressLabel : "",
                    addressId    : "",
                    obfuscatedId : "",
                    countryCode  : "US",
                    zipCode      : "60090",
                    state        : "IL",
                    city         : "WHEELING",
                    district     : "",
                    locationType : "IP2LOCATION"
                });
            });


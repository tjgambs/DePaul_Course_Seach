            </ol></div></div><div class="a-carousel-col a-carousel-right"><a class="a-button a-button-image a-carousel-button a-carousel-goto-nextpage" tabindex="0" href="#"><span class="a-button-inner"><i class="a-icon a-icon-next"><span class="a-icon-alt">Next</span></i></span></a></div></div></div>
        </div>
        
    
        </div>
        
        
    










<!--AMZNJQSECTION-->


  
  
  
  

  
  
    


  


  
  



























 





<div id="cpsia-product-safety-warning_feature_div">
</div>




    

    





 




<hr noshade="true" size="1" class="bucketDivider"/>

<div id="pd-available"></div>






<script type="text/javascript">
var ProductDescriptionIframeResize = {};
(function () {
  var d = document;
  var obj = this;
  var nTries = 1;
  var hasCustomWidth = 0;
  var MAX_TRIES = 20;
  obj.iframeId = 'product-description-iframe';
  obj.appendTo = 'iframe-wrapper';
  function getDocHeight (doc) {
    var contentDiv = doc.getElementById("pdIframeContent");
    var docHeight = 0;
    if(contentDiv){
      docHeight = Math.max(
        contentDiv.scrollHeight,
        contentDiv.offsetHeight,
        contentDiv.clientHeight
      );
    }
    return docHeight;
  }  
  function isElementOnRight(pdRef, el) {
    var ret=0;
    try {
      if(pdRef, el) {
        if((el.offsetTop < pdRef.offsetTop) && (el.offsetTop + el.offsetHeight > pdRef.offsetTop)) {
          ret = 1;
        }
      }
    } catch (e) {}
    return ret;
  }
  function setWidth(iframeRef, el) {
    var elLeft, w, minW = 450;
    try {
      elLeft = el.offsetLeft;
      w = elLeft - 50;
      if(w > minW) {
        iframeRef.style.width = w + 'px';
        hasCustomWidth = 1;
      }
    } catch (e) {}
  }
  function setIframeWidth(ifrmId) {
    var iframeRef = d.getElementById(ifrmId),
        pdRef = d.getElementById("pd-available"),
        rightCol = d.getElementById("rightCol"),
        rightRail = d.getElementById("rightRail");
    if(isElementOnRight(pdRef, rightRail)) {
      setWidth(iframeRef, rightRail); 
    } else if (isElementOnRight(pdRef, rightCol)) {
      setWidth(iframeRef, rightCol);
    }
  }
  function setIframeHeight (ifrmId) {
    var iframeDoc, iframeRef = d.getElementById(ifrmId);
    try {
      iframeDoc = iframeRef.contentWindow.document;
    } catch (e) {
      try {
        iframeDoc = iframeRef.contentDocument;
      } catch (ex) {}
    }
    if (iframeDoc) {
      var h = getDocHeight(iframeDoc);
      if (h && h != 0) {
        iframeRef.style.height = h + 'px';
      } else { 
        if (nTries < MAX_TRIES) {
          nTries++;
          setTimeout(function () {
            setIframeHeight(ifrmId);
          }, 50); 
        }
      }
    }
  }
  obj.iframeload = function () {
    var iframe = d.getElementById(obj.iframeId);
    iframe.style.display = '';
    iframe.style.height = "1px";
    setTimeout(function () {
      if(obj.needWidthAdjust) {
        setIframeWidth(obj.iframeId);
      }
      setIframeHeight(obj.iframeId);
      var onloadCallback = obj.onloadCallback;
      if(typeof onloadCallback === "function") {
        onloadCallback();
      }
    }, 50); 

  }
  obj.createIframe = function ( iframeId, onloadFn ) {
    var iframe = d.createElement('iframe');
    iframe.id = iframeId;
    iframe.className = "ap_never_hide";
    iframe.width = "100%";
    iframe.scrolling="no";
    iframe.frameBorder="0";
    if ( iframe.addEventListener )
      iframe.addEventListener("load", onloadFn, false);
    else if ( iframe.attachEvent )
      iframe.attachEvent("onload", onloadFn);
    else iframe.onload = onloadFn;
    return iframe; 
  }
  obj.initialize = function (onloadCallback, needWidthAdjust) {
    var iframeContent = "%0A%20%20%0A%0A%0A%0A%3Chtml%3E%0A%20%20%3Chead%3E%0A%20%20%20%20%3Cstyle%20type%3D'text%2Fcss'%3E%0A%0A%0A%20h2%20%7B%0A%20%20%20%20%20%20color%3A%23333%20!important%3B%0A%20%20%20%20%20%20font-size%3A21px%20!important%3B%0A%20%20%20%20%20%20line-height%3A%201.3%3B%0A%20%20%20%20%20%20padding-bottom%3A%204px%3B%0A%20%20%20%20%20%20font-weight%3A%20normal%3B%0A%20%20%20%20%20%20font-family%3AArial%2C%20sans-serif%3B%0A%20%20%20%20%20%20margin%3A%200px%3B%0A%20%7D%0A%20%20%20%20%0Abody%20%7B%0A%20%20background-color%3A%20%23FFFFFF%3B%0A%20%20margin%3A%200px%3B%0A%20%20height%3A%20100%25%3B%0A%20%20width%3A%20100%25%3B%0A%20%20color%3A%20%23333333%3B%0A%20%20word-wrap%3A%20break-word%3B%20%0A%20%20font-family%3A%20verdana%2Carial%2Chelvetica%2Csans-serif%3B%0A%20%20font-size%3A%20small%3B%0A%7D%0A%0Atd%2C%20th%20%7B%0A%20%20font-family%3A%20verdana%2Carial%2Chelvetica%2Csans-serif%3B%0A%20%20font-size%3A%20small%3B%0A%7D%0A%0Ahr%20%7B%0A%20%20border-top%3A%201px%20dashed%20%23999999%3B%0A%20%20height%3A%201px%3B%0A%20%20color%3A%20%23FFFFFF%3B%0A%20%20margin%3A%203px%200px%3B%0A%20%20border-style%3A%20dashed%20none%20none%20none%3B%0A%20%20background-color%3A%20white%0A%7D%0Aul%20%7B%20%0A%20%20list-style-type%3A%20none%3B%0A%20%20margin%3A%200px%3B%20%0A%20%20padding%3A%200px%3B%20%0A%7D%0Aul%20li%20%7B%0A%20%20margin%3A%200.5em%200em%3B%0A%7D%0Aul%20li%20ul%20%7B%0A%20%20list-style-type%3A%20none%3B%0A%20%20margin-left%3A%2025px%3B%0A%7D%0Aul%20li%20ul%20li%20%7B%0A%20%20margin%3A%200em%3B%0A%7D%0Adiv.bucket%20%7B%20padding%3A%205px%200em%3B%20%7D%0Adiv.bucket%20div.content%20%7B%20margin%3A%200.5em%200px%200em%2025px%3B%20%7D%0Ahr%20%7B%20margin-left%3A%200px%3B%20%7D%0A.bucket%20h3%20%7B%0A%20%20color%3A%20%23000%3B%0A%20%20font-size%3A%201em%3B%0A%20%20font-weight%3A%20bold%3B%0A%20%20margin%3A%200px%200px%200.25em%200px%3B%0A%7D%0Ahr.bucketDivider%20%7B%20clear%3A%20left%3B%20%7D%0Adiv.bucket%20%7B%20padding%3A%205px%200em%3B%20%7D%0A.disclaim%20%7B%0A%20%20margin-top%3A%203px%3B%0A%20%20font-size%3A%2010px%3B%0A%20%20color%3A%20%23888%3B%0A%7D%0Abody%20hr%20%7B%0A%20%20border-top%3A%201px%20solid%20%23CCCCCC%3B%0A%7D%0Aa%3Alink%20%7B%0A%20%20color%3A%20%23003399%3B%0A%20%20text-decoration%3A%20underline%3B%0A%7D%0Aa%3Ahover%20%7B%0A%20%20color%3A%20%23CC6600%3B%0A%20%20text-decoration%3A%20none%3B%0A%7D%0Aa%3Aactive%20%7B%0A%20%20color%3A%20%23FF9933%3B%0A%7D%0Aa%3Avisited%20%7B%0A%20%20color%3A%20%23996633%3B%0A%7D%0A%0A%0A%0A%23productDescription%20h2.productDescriptionHeader%20%7B%0A%20%20%20%20margin-bottom%3A%200em%3B%09%0A%7D%0A%0A%23productDescription%20.emptyClear%20%7B%0A%20%20%20%20clear%3Aleft%3B%0A%20%20%20%20height%3A0px%3B%0A%20%20%20%20font-size%3A0px%3B%0A%7D%0A%0A%23productDescription%20div.productDescriptionWrapper%20%7B%0A%20%20%20%20margin%3A%200%200%201em%200%3B%09%09%0A%7D%0A%0A%23productDescription%20h3.productDescriptionSource%20%7B%0A%20%20%20%20font-weight%3Anormal%3B%0A%20%20%20%20color%3A%23333333%3B%0A%20%20%20%20font-size%3A1.23em%3B%0A%20%20%20%20margin%3A%20.75em%200%20.375em%20-15px%3B%0A%20%20%20%20clear%3Aleft%3B%0A%7D%0A%0A%23productDescription%20.seeAll%20%7B%0A%09margin-top%3A%201.25em%3B%20%0A%09margin-left%3A%20-15px%3B%20%0A%7D%0A%0A%23productDescription%20ul%2C%20%23technicalProductFeatures%20ul%20%7B%20%0A%20%20list-style-type%3A%20disc%3B%20%0A%20%20margin%3A%201.12em%200%3B%20%0A%20%20margin-left%3A%2020px%3B%20%0A%7D%0A%0A%23productDescription%20ul%20li%20%7B%20%0A%20%20margin%3A%200%200%200%2020px%3B%20%0A%7D%0A%0A%23productDescription%20ul%20li%20ul%20%7B%20%0A%20%20list-style-type%3A%20disc%3B%20%0A%20%20margin-left%3A%2020px%3B%20%0A%7D%0A%0A%23productDescription%20ul%20li%20ul%20li%20%7B%20%0A%20%20margin%3A%200%200%200%2020px%3B%20%0A%7D%0A%0A%23productDescription%20.aplus%20h4%2C%20%23productDescription%20.aplus%20h5%20%7B%0A%20%20%20%20margin%3A%200%200%20.75em%200%3B%0A%20%20%20%20font-size%3A%201em%3B%0A%7D%0A%0A%23productDescription%20.aplus%20h4%20%7B%0A%20%20%20%20color%3A%20%23CC6600%3B%0A%7D%0A%0A%23productDescription%20.aplus%20p%20%7B%0A%20%20%20%20margin%3A%200%200%201em%200%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.break%20%7B%0A%20%20%20%20clear%3Aboth%3B%0A%20%20%20%20height%3A0px%3B%0A%20%20%20%20font-size%3A0px%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.spacer%20%7B%0A%20%20%20%20margin-bottom%3A%2013px%3B%0A%7D%0A%0A%23productDescription%20.aplus%20img%20%7B%0A%20%20%20%20border%3Anone%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.leftImage%2C%20%23productDescription%20.aplus%20.rightImage%2C%20%23productDescription%20.aplus%20.centerImage%20%7B%0A%20%20%20%20margin-bottom%3A%201em%3B%0A%20%20%20%20margin-top%3A%200%3B%0A%20%20%20%20text-align%3Acenter%3B%0A%20%20%20%20vertical-align%3Atop%3B%0A%7D%0A%0A%0A%23productDescription%20.aplus%20.leftImage%20%7B%0A%20%20%20%20margin-right%3A%2015px%3B%0A%20%20%20%20float%3Aleft%3B%0A%20%20%20%20clear%3Aleft%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.rightImage%20%7B%0A%20%20%20%20margin-left%3A%2015px%3B%0A%20%20%20%20float%3Aright%3B%0A%20%20%20%20clear%3Aright%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.imageCaption%20%7B%0A%20%20%20%20clear%3Aboth%3B%0A%20%20%20%20padding%3A%20.5em%20.5em%200%20.5em%3B%0A%20%20%20%20font-size%3A%20.846em%3B%0A%20%20%20%20display%3A%20block%3B%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20%7B%20%0A%09border-collapse%3A%20collapse%3B%20%0A%09margin-bottom%3A%201.25em%3B%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20th%20%7B%20%0A%09font-weight%3A%20bold%3B%20%0A%09background%3A%20%23F7F7F7%3B%20%0A%09border-style%3Asolid%3B%20%0A%09border-color%3A%20%23CCCCCC%3B%20%0A%09border-width%3A0%200%201px%201px%3B%20%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20td%20%7B%20%0A%09border-left%3A%201px%20solid%20%23CCC%3B%20%0A%09border-bottom%3A%201px%20dotted%20%23CCC%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20th%2C%20%23productDescription%20.aplus%20table.data%20td%0A%7B%20%0A%09padding%3A3px%2010px%3B%20%0A%09text-align%3Aleft%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20tfoot%20%7B%20%0A%09font-style%3A%20italic%3B%20%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20caption%20%7B%0A%09background%3A%20%23eee%3B%20%0A%09font-size%3A%20.8125em%3B%0A%7D%0A%0A%23productDescription%20.aplus%20table.data%20tr%20td%3Afirst-child%2C%20%23productDescription%20.aplus%20table.data%20tr%20th%3Afirst-child%20%7B%0A%09border-left-width%3A0px%3B%0A%7D%0A%0A%23productDescription%20.aplus%20ul%20%7B%0A%09margin%3A0%200%201em%200%3B%0A%7D%0A%0A%0A%23productDescription%20.aplus%20.center%20%7B%0A%09text-align%3A%20center%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.right%20%7B%0A%09text-align%3A%20right%3B%0A%7D%0A%0A%23productDescription%20.aplus%20%20.sixth-col%2C%0A%23productDescription%20.aplus%20.fourth-col%2C%0A%23productDescription%20.aplus%20.third-col%2C%0A%23productDescription%20.aplus%20.half-col%2C%0A%23productDescription%20.aplus%20.two-third-col%2C%0A%23productDescription%20.aplus%20.three-fourth-col%2C%0A%23productDescription%20.aplus%20.one-col%20%7B%0A%20%20%20%20float%3Aleft%3B%0A%20%20%20%20margin-right%3A%201.6760%25%3B%0A%20%20%20%20overflow%3A%20hidden%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.last%20%7B%0A%20%20%20%20margin-right%3A0px%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.sixth-col%20%7B%0A%20%20%20%20width%3A%2015.080%25%3B%0A%7D%0A%23productDescription%20.aplus%20.fourth-col%20%7B%0A%20%20%20%20width%3A%2023.4637%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.third-col%20%7B%0A%20%20%20%20width%3A%2031.8436%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.half-col%20%7B%0A%20%20%20%20width%3A%2048.6034%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.two-third-col%20%7B%0A%20%20%20%20width%3A%2065.3631%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.three-fourth-col%20%7B%0A%20%20%20%20width%3A%2073.7430%25%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.one-col%20%7B%0A%20%20%20%20width%3A%2098.8827%25%3B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.last%20%7B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20100%25%3B%0A%20%20%20%20min-width%3A%20895px%3B%0A%7D%0A%0A*%20html%20%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20expression((document.body.clientWidth%20%3C%20936)%20%3F%20%22895px%22%20%3A%20%22100%25%22%20)%3B%0A%7D%0A%0A%20%20%20%20%3C%2Fstyle%3E%0A%20%20%20%20%3Cbase%20target%3D'_top'%20%2F%3E%0A%20%20%3C%2Fhead%3E%0A%20%20%3Cbody%3E%0A%20%20%3Cdiv%20id%3D%22pdIframeContent%22%3E%0A%0A%0A%0A%0A%20%0A%0A%0A%0A%0A%3Cdiv%20id%3D%22biss-product-description-and-details%22%3E%3C%2Fdiv%3E%0A%0A%3Cdiv%20class%3D%22bucket%22%20id%3D%22productDescription%22%3E%0A%20%20%3Ch2%3EEditorial%20Reviews%3C%2Fh2%3E%0A%0A%3Cdiv%20class%3D%22content%22%3E%0A%0A%0A%20%20%20%20%0A%20%20%20%20%20%20%20%3Ch3%20class%3D%22productDescriptionSource%22%3EAbout%20the%20Author%3C%2Fh3%3E%0A%20%20%20%20%20%20%20%3Cdiv%20class%3D%22productDescriptionWrapper%22%3E%0A%20%20%20%20%20%20%20Stanley%20Baran%20earned%20his%20Ph.D.%20in%20communication%20research%20at%20the%20University%20of%20Massachusetts%20after%20taking%20his%20M.A.%20in%20journalism%20at%20Pennsylvania%20State%20University.%20He%20taught%20for%204%20years%20at%20Cleveland%20State%20University%2C%20eventually%20moving%20to%20the%20University%20of%20Texas.%20He%20led%20the%20Department%20of%20Radio-TV-Film%26%23x2019%3Bs%20graduate%20program%20for%206%20of%20his%209%20years%20in%20Austin%20and%20won%20numerous%20teaching%20awards%20there%2C%20including%20the%20AMOCO%20Teaching%20Excellence%20Award%20as%20the%20best%20instructor%20on%20that%2040%2C000-student%20campus%2C%20the%20College%20of%20Communication%26%23x2019%3Bs%20Teaching%20Excellence%20Award%20as%20that%20college%26%23x2019%3Bs%20outstanding%20professor%2C%20and%20Utmost%20Magazine%26%23x2019%3Bs%20Student%20Poll%20for%20best%20instructor.%20Dr.%20Baran%20moved%20to%20San%20Jose%20State%20University%20in%201987%20and%20served%209%20years%20as%20chair%20of%20the%20Department%20of%20Television%2C%20Radio%2C%20Film%2C%20and%20Theatre.%20At%20SJSU%20he%20was%20named%20President%26%23x2019%3Bs%20Scholar%20as%20the%20University%26%23x2019%3Bs%20outstanding%20researcher.%20Now%2C%20he%20teaches%20at%20Bryant%20University%2C%20where%20he%20is%20the%20founding%20chair%20of%20that%20school%26%23x2019%3Bs%20Communication%20Department.%20Among%20his%20other%20experiences%20shaping%20this%20book%20are%20service%20as%20a%20Fulbright%20Scholar%20and%20his%20many%20years%20of%20professional%20activity%20in%20audience%20research%2C%20writing%20for%20radio%2C%20and%20producing%20for%20television.%20Dr.%20Baran%20has%20published%2010%20books%20and%20scores%20of%20scholarly%20articles%20and%20sits%20or%20has%20sat%20on%20the%20editorial%20boards%20of%20six%20journals.%20His%20work%20has%20been%20translated%20into%20half-a-dozen%20languages.%20He%20is%20a%20skilled%20boater%20and%20a%20tenor%20saxophonist%20for%20the%20Wakefield%2C%20Rhode%20Island%2C%20Concert%20Band.%20He%20is%20married%20to%20Susan%20Baran%20and%20has%20two%20very%20cool%20children%2C%20Matt%20and%20Jordan%2C%20who%20grew%20up%20much%20faster%20than%20he%20wanted.%0A%20%20%20%20%20%20%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fdiv%3E%0A%3C%2Fdiv%3E%0A%0A%0A%0A%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fbody%3E%0A%3C%2Fhtml%3E%0A";
    obj.onloadCallback = onloadCallback;
    obj.needWidthAdjust = needWidthAdjust;
    var iframe = obj.createIframe (obj.iframeId, obj.iframeload);
    var wrapper = d.getElementById(obj.appendTo);
    wrapper.innerHTML = '';

    var anchor = d.createElement('div');
    anchor.id = 'biss-product-description-and-details';
    wrapper.appendChild(anchor);

    iframe = wrapper.appendChild(iframe);
    var doc;
    try {
      doc = iframe.contentWindow.document;
    } catch (e) {
      try {
        doc = iframe.contentDocument;
      } catch (ex) {}
    }
    if (doc && doc.open) {
      obj.doc = doc;
      doc.open().write(decodeURIComponent(iframeContent));
      doc.close();
    }
  }
  obj.resizeIframe = function() {
    if(hasCustomWidth) {
      setIframeWidth(obj.iframeId);
    }
    nTries = 1;
    setIframeHeight(obj.iframeId);
  }
}).apply(ProductDescriptionIframeResize);

amznJQ.onCompletion('amznJQ.criticalFeature', function () {
  amznJQ.available('jQuery',function(){
    setTimeout(function() {
      var iframeLoadFn = function() {
        var $ = jQuery;
        $("#pd-available").trigger("pdAvailable");
        $(window).resize(ProductDescriptionIframeResize.resizeIframe);
      };
      ProductDescriptionIframeResize.initialize(iframeLoadFn, 1);
    }, 50);
  });
});
</script>




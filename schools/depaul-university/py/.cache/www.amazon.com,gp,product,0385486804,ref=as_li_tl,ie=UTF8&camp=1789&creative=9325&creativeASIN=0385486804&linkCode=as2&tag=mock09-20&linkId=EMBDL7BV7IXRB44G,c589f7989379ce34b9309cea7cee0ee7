<div id="pd-available"></div>






<script type="text/javascript">
var ProductDescriptionIframeResize = {};
(function () {
  var d = document;
  var obj = this;
  var nTries = 1;
  var hasCustomWidth = 0;
  var MAX_TRIES = 20;
  obj.iframeId = 'product-description-iframe';
  obj.appendTo = 'iframe-wrapper';
  function getDocHeight (doc) {
    var contentDiv = doc.getElementById("pdIframeContent");
    var docHeight = 0;
    if(contentDiv){
      docHeight = Math.max(
        contentDiv.scrollHeight,
        contentDiv.offsetHeight,
        contentDiv.clientHeight
      );
    }
    return docHeight;
  }  
  function isElementOnRight(pdRef, el) {
    var ret=0;
    try {
      if(pdRef, el) {
        if((el.offsetTop < pdRef.offsetTop) && (el.offsetTop + el.offsetHeight > pdRef.offsetTop)) {
          ret = 1;
        }
      }
    } catch (e) {}
    return ret;
  }
  function setWidth(iframeRef, el) {
    var elLeft, w, minW = 450;
    try {
      elLeft = el.offsetLeft;
      w = elLeft - 50;
      if(w > minW) {
        iframeRef.style.width = w + 'px';
        hasCustomWidth = 1;
      }
    } catch (e) {}
  }
  function setIframeWidth(ifrmId) {
    var iframeRef = d.getElementById(ifrmId),
        pdRef = d.getElementById("pd-available"),
        rightCol = d.getElementById("rightCol"),
        rightRail = d.getElementById("rightRail");
    if(isElementOnRight(pdRef, rightRail)) {
      setWidth(iframeRef, rightRail); 
    } else if (isElementOnRight(pdRef, rightCol)) {
      setWidth(iframeRef, rightCol);
    }
  }
  function setIframeHeight (ifrmId) {
    var iframeDoc, iframeRef = d.getElementById(ifrmId);
    try {
      iframeDoc = iframeRef.contentWindow.document;
    } catch (e) {
      try {
        iframeDoc = iframeRef.contentDocument;
      } catch (ex) {}
    }
    if (iframeDoc) {
      var h = getDocHeight(iframeDoc);
      if (h && h != 0) {
        iframeRef.style.height = h + 'px';
      } else { 
        if (nTries < MAX_TRIES) {
          nTries++;
          setTimeout(function () {
            setIframeHeight(ifrmId);
          }, 50); 
        }
      }
    }
  }
  obj.iframeload = function () {
    var iframe = d.getElementById(obj.iframeId);
    iframe.style.display = '';
    iframe.style.height = "1px";
    setTimeout(function () {
      if(obj.needWidthAdjust) {
        setIframeWidth(obj.iframeId);
      }
      setIframeHeight(obj.iframeId);
      var onloadCallback = obj.onloadCallback;
      if(typeof onloadCallback === "function") {
        onloadCallback();
      }
    }, 50); 

  }
  obj.createIframe = function ( iframeId, onloadFn ) {
    var iframe = d.createElement('iframe');
    iframe.id = iframeId;
    iframe.className = "ap_never_hide";
    iframe.width = "100%";
    iframe.scrolling="no";
    iframe.frameBorder="0";
    if ( iframe.addEventListener )
      iframe.addEventListener("load", onloadFn, false);
    else if ( iframe.attachEvent )
      iframe.attachEvent("onload", onloadFn);
    else iframe.onload = onloadFn;
    return iframe; 
  }
  obj.initialize = function (onloadCallback, needWidthAdjust) {
    var iframeContent = "%0A%20%20%0A%0A%0A%0A%3Chtml%3E%0A%20%20%3Chead%3E%0A%20%20%20%20%3Cstyle%20type%3D'text%2Fcss'%3E%0A%0A%0A%20h2%20%7B%0A%20%20%20%20%20%20color%3A%23333%20!important%3B%0A%20%20%20%20%20%20font-size%3A21px%20!important%3B%0A%20%20%20%20%20%20line-height%3A%201.3%3B%0A%20%20%20%20%20%20padding-bottom%3A%204px%3B%0A%20%20%20%20%20%20font-weight%3A%20normal%3B%0A%20%20%20%20%20%20font-family%3AArial%2C%20sans-serif%3B%0A%20%20%20%20%20%20margin%3A%200px%3B%0A%20%7D%0A%20%20%20%20%0Abody%20%7B%0A%20%20background-color%3A%20%23FFFFFF%3B%0A%20%20margin%3A%200px%3B%0A%20%20height%3A%20100%25%3B%0A%20%20width%3A%20100%25%3B%0A%20%20color%3A%20%23333333%3B%0A%20%20word-wrap%3A%20break-word%3B%20%0A%20%20font-family%3A%20verdana%2Carial%2Chelvetica%2Csans-serif%3B%0A%20%20font-size%3A%20small%3B%0A%7D%0A%0Atd%2C%20th%20%7B%0A%20%20font-family%3A%20verdana%2Carial%2Chelvetica%2Csans-serif%3B%0A%20%20font-size%3A%20small%3B%0A%7D%0A%0Ahr%20%7B%0A%20%20border-top%3A%201px%20dashed%20%23999999%3B%0A%20%20height%3A%201px%3B%0A%20%20color%3A%20%23FFFFFF%3B%0A%20%20margin%3A%203px%200px%3B%0A%20%20border-style%3A%20dashed%20none%20none%20none%3B%0A%20%20background-color%3A%20white%0A%7D%0Aul%20%7B%20%0A%20%20list-style-type%3A%20none%3B%0A%20%20margin%3A%200px%3B%20%0A%20%20padding%3A%200px%3B%20%0A%7D%0Aul%20li%20%7B%0A%20%20margin%3A%200.5em%200em%3B%0A%7D%0Aul%20li%20ul%20%7B%0A%20%20list-style-type%3A%20none%3B%0A%20%20margin-left%3A%2025px%3B%0A%7D%0Aul%20li%20ul%20li%20%7B%0A%20%20margin%3A%200em%3B%0A%7D%0Adiv.bucket%20%7B%20padding%3A%205px%200em%3B%20%7D%0Adiv.bucket%20div.content%20%7B%20margin%3A%200.5em%200px%200em%2025px%3B%20%7D%0Ahr%20%7B%20margin-left%3A%200px%3B%20%7D%0A.bucket%20h3%20%7B%0A%20%20color%3A%20%23000%3B%0A%20%20font-size%3A%201em%3B%0A%20%20font-weight%3A%20bold%3B%0A%20%20margin%3A%200px%200px%200.25em%200px%3B%0A%7D%0Ahr.bucketDivider%20%7B%20clear%3A%20left%3B%20%7D%0Adiv.bucket%20%7B%20padding%3A%205px%200em%3B%20%7D%0A.di
        function dumpHtml( obj, newAdsHtml, topHtml, bottomHtml, divParent ) {
              var i = 0;
              var completeAdsHtml = '';
              for( x in obj ) {
                  var adChunks = newAdsHtml.split( adPropertiesDelimiter );
                  for( var j = 0; j<adChunks.length; j++ ) {
                      if( typeof obj[i][adChunks[j]] != "undefined" ) {
                          adChunks[j] = obj[i][adChunks[j]];
                      }
                  }
                  completeAdsHtml += adChunks.join("");
                  i++;
              }
              newAdsHtml = topHtml + completeAdsHtml + bottomHtml;
              $('#' + divParent).replaceWith(newAdsHtml);
        }
	});

	P.when('jQuery','ready').execute(function($){
	    var jsonAds = '[{"redirectUrl":"http://rd.a9.com/srv/redirect/?info=ACPLFjbTcndnQ4nGNVmGE27z1C5bdWpETVpdLLhqNnp0tMDcG43kVEYd3RWSNvlzb.6SOQSz03kV7tC1q.pmfvc.1MWS-FU62PCBQBvWe7H-R9IStyBRBYcie1I9m1hRkOotv6VYt8-ZBNh14K4yL47ixMFo-L9bsn990TWt2pmRdrDA9f2-7QZZoRbeBWQT-eTPmKDv1koJf.IXGD69xer9HkdaJU6.rdAL.9cd.P5qIAd0bjf9-3QHcj0nBCz8If6Nl6D5bmmeCBqQB1bTM-90d73hVZPrAxAFg0lLtBTuiCYXKyDSZ-5gA8Ov53aZeiWpOSc5w1e7tMobw-Epia88LEV9WACFxI25XK6uvAGhWatqXIEjTRLzctApVJgsBcbG3a7LfhL2MfRrvHCT.u3hpSsaY070kIaA3rZMXpKfDWA3LvPvBjHqkRUkf.Xmt0qL0IztFXVcLIYX.QXwcmB1mLgBXKh-C3dsJffxO3mo8VFL7qqAqtRcybxo.muLQqVR2Y4z8UmU9yYYCQ83x604kgIX7hYUtt1CC5SjBFTIw.RmVfkyoiMoWDxV3YWOsk2OZiJK1ZHFtP8Fn3fSG03u839pGTXpfVj55hlXvdlHAwh1HxSob0Gwx6Fb11hZm10Op-IFVernEFiQyr4swgAwPGM78oVWCBp8ZYSlUUbmW2uZyaUZfbhNcWHoBnkr6.hwWq5tnpkAZ-4iVegRmrmC0BU3aEX1NQ__&awt=1&s=","visibleUrl":"www.harpercollins.com/The<b>Wild</b>Truth","provider":"g","description":"The other side to the powerful story about Chris McCandless","title":"The <b>Wild</b> Truth- now in pb"}]';
	    var feedbackFormAvailable = 0;
	    function showSLF() {
	         if(!feedbackFormAvailable) {
	           // Make a request to the handler with new ads 
	           $.post('/gp/product/aui_feedback_form_handler.html', { 'sid': '184-5528287-5166848','jsonAds' : escape(jsonAds) } ,function(data) {
	               $('#FeedbackFormDiv').html(data);
	               $('#FeedbackFormDiv').show();
	               $('#ShowFeedbackLinkDiv').hide();
	               $('#ResponseFeedbackLinkDiv').hide(); 
	               feedbackFormAvailable = 1;
	           });
	         } else {
	           $('#reports-ads-abuse-form').show();
	           $('#FeedbackFormDiv').show();
	           $('#reports-ads-abuse').show();
	           $('#ShowFeedbackLinkDiv').hide();
	           $('#ResponseFeedbackLinkDiv').hide(); 
	         }
	    }
	    $('#LetUsKnowLink').click(showSLF);
	    $('#ShowFeedbackLinkDiv').css('opacity','1');
	    $('#ShowFeedbackLinkDiv').css('pointer-events','auto');
	});


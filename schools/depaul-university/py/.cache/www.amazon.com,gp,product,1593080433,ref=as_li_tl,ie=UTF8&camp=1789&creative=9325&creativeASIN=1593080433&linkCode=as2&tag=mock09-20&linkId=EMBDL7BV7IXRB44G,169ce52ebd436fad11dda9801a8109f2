    }
    var data = {
        
        'litbData' : {
            'hasLitb' : false,
            'litbReftag' : "sib_dp_pt"
        },
        'audibleData' : audibleData,
        'windowWidthThreshold' : 1150,
        'configWidths' : [200, 260],
        'holderRatio' : 0.75,
        'containerMargin' : 15,
        'flipLinkMinHeight' : 0,
        'frontImageWidth' : 228,
        'frontImageHeight' : 346,
        'frontImageAspectRatio' : 0.6589595375722543,
        'imageGalleryData' : [{"mainUrl":"http://ecx.images-amazon.com/images/I/414So2CNyzL.jpg","dimensions":[328,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/414So2CNyzL._AC_SX75_CR,0,0,75,75_.jpg"}],
        'centerColMargin' : 40,
        'disableResize' : false
    };
    return data;
});

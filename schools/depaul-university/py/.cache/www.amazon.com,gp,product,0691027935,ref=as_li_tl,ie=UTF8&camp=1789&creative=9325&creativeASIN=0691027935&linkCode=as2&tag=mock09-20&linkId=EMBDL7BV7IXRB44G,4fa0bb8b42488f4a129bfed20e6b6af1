    function handleSitbLogoClick(event) {
        var eventData = event.data;
        var hasLitb = eventData.hasLitb;
        
        if (hasLitb && typeof(SitbReader) != 'undefined') {
            SitbReader.LightboxActions.openReader(eventData.litbReftag);
        }
    };
    

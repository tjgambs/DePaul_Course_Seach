  amznJQ.onReady('popover', function() {
    (function($) {
      if($.fn.acrPopover) return;
      var popoverConfig = {
        showOnHover:true,
        showCloseButton: true,
        width: null,
        location:'bottom',
        locationAlign:'left',
        locationOffset:[-20,0],
        paddingLeft: 15,
        paddingBottom: 5,

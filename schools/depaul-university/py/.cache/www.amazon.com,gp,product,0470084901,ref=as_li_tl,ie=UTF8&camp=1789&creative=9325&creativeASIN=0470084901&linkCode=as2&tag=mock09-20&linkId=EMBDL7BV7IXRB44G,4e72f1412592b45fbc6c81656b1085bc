     progid:DXImageTransform.Microsoft.gradient(
     startColorstr='#03ffffff',
     endColorstr='#ffffff', GradientType=0
   );
   background: linear-gradient(
     bottom,
     rgb(255, 255, 255) 15%,
     rgba(255, 255, 255, 0) 100%
   );
   display: block;
}


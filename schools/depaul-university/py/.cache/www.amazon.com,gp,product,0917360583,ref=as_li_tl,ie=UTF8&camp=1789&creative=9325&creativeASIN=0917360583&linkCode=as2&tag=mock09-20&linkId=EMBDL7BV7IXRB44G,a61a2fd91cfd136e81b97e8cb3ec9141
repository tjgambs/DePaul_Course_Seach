            'audioSampleSrc' : "",
            'flashPlayerUrl' : ""
        }
    }
    var data = {
        
        'litbData' : {
            'hasLitb' : false,
            'litbReftag' : "sib_dp_pt"
        },
        'audibleData' : audibleData,
        'windowWidthThreshold' : 1150,
        'configWidths' : [200, 260],
        'holderRatio' : 0.75,
        'containerMargin' : 15,
        'flipLinkMinHeight' : 0,
        'frontImageWidth' : 260,
        'frontImageHeight' : 346,
        'frontImageAspectRatio' : 0.7514450867052023,
        'imageGalleryData' : [{"mainUrl":"http://ecx.images-amazon.com/images/I/9104dXm0F3L.jpg","dimensions":[1920,2560],"thumbUrl":"http://ecx.images-amazon.com/images/I/9104dXm0F3L._AC_SX75_CR,0,0,75,75_.jpg"}],
        'centerColMargin' : 40,
        'disableResize' : false
    };
    return data;
});

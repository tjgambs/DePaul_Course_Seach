            handleSitbLogoClick(event);
        }
    }
    
    A.declarative("sitb-logo-click", ['click'], handleSitbLogoClick);
    A.declarative("minimal-main-image-click", ['click'], handleMinimalMainImageClick);
});
</script>





            



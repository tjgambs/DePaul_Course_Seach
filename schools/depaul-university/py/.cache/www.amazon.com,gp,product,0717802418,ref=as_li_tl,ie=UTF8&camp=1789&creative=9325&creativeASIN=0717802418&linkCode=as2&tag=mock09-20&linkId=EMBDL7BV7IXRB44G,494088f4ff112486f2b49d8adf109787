            'flashPlayerUrl' : "http://g-ecx.images-amazon.com/images/G/01/zeitgeist/mp3player/swf/zgMp3Player-1.0._CB212274098_.swf"
        }
    }
    var data = {
        
        'litbData' : {
            'hasLitb' : false,
            'litbReftag' : "sib_dp_pt"
        },
        'audibleData' : audibleData,
        'windowWidthThreshold' : 1150,
        'configWidths' : [200, 260],
        'holderRatio' : 0.75,
        'containerMargin' : 15,
        'flipLinkMinHeight' : 25,
        'frontImageWidth' : 230,
        'frontImageHeight' : 346,
        'frontImageAspectRatio' : 0.6647398843930635,
        'imageGalleryData' : [{"mainUrl":"http://ecx.images-amazon.com/images/I/91zdHwFmpRL.jpg","dimensions":[1466,2211],"thumbUrl":"http://ecx.images-amazon.com/images/I/91zdHwFmpRL._AC_SX75_CR,0,0,75,75_.jpg"},{"mainUrl":"http://ecx.images-amazon.com/images/I/41VPTNSUliL.jpg","dimensions":[375,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/41VPTNSUliL._AC_SX75_CR,0,0,75,75_.jpg"}],
        'centerColMargin' : 40,

e-col%20%7B%0A%20%20%20%20width%3A%2098.8827%25%3B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.last%20%7B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20100%25%3B%0A%20%20%20%20min-width%3A%20895px%3B%0A%7D%0A%0A*%20html%20%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20expression((document.body.clientWidth%20%3C%20936)%20%3F%20%22895px%22%20%3A%20%22100%25%22%20)%3B%0A%7D%0A%0A%20%20%20%20%3C%2Fstyle%3E%0A%20%20%20%20%3Cbase%20target%3D'_top'%20%2F%3E%0A%20%20%3C%2Fhead%3E%0A%20%20%3Cbody%3E%0A%20%20%3Cdiv%20id%3D%22pdIframeContent%22%3E%0A%0A%0A%0A%0A%20%0A%0A%0A%0A%0A%3Cdiv%20id%3D%22biss-product-description-and-details%22%3E%3C%2Fdiv%3E%0A%0A%3Cdiv%20class%3D%22bucket%22%20id%3D%22productDescription%22%3E%0A%20%20%3Ch2%3EEditorial%20Reviews%3C%2Fh2%3E%0A%0A%3Cdiv%20class%3D%22content%22%3E%0A%0A%0A%20%20%20%20%0A%20%20%20%20%20%20%20%3Ch3%20class%3D%22productDescriptionSource%22%3EReview%3C%2Fh3%3E%0A%20%20%20%20%20%20%20%3Cdiv%20class%3D%22productDescriptionWrapper%22%3E%0A%20%20%20%20%20%20%20%26%2334%3BThis%20is%20a%20first-rate%20edition%20of%20an%20increasingly%20important%20play.%20Mathew%20Martin's%20editing%20of%20the%20quarto%20text%20of%20Edward%20the%20Second%20is%20detailed%20and%20thoughtful%2C%20with%20copious%2C%20insightful%20annotations%2C%20and%20his%20critical%20introduction%20lucidly%20explores%20the%20play's%20theatrical%20contexts%2C%20historiographical%20concerns%2C%20and%20thematic%20imperatives.%20The%20extensive%20appendices%20that%20conclude%20the%20volume%20are%20invaluable%20for%20understanding%20the%20larger%20historical%2C%20political%2C%20and%20sexual%20contexts%20of%20the%20work.%20All%20in%20all%2C%20this%20is%20an%20edition%20that%20will%20greatly%20benefit%20both%20the%20student%20reader%20and%20the%20experienced%20scholar.%26%2334%3B%20(Ian%20Munro%20)%3Cbr%20%2F%3E%3Cbr%20%2F%3E%26%2334%3BMathew%20Martin's%20new%20edition%20of%20Edward%20the%20Second%20will%20serve%20well%20the%20needs%20of%20students.%20The%20introduction%20contains%20a%20succinct%20and%20helpful%20summary%20of%20the%20pertinent%20aspects%20of%20Marlowe's%20life%20and%20of%20the%20practical%20concerns%20of%20the%20Elizabethan%20stage%2C%20details%20the%20reign%20of%20the%20historical%20Edward%20II%2C%20and%20considers%20early%20modern%20and%20postmodern%20evaluations%20of%20%26%2334%3Bsodomitical%26%2334%3B%20relationships.%20Appendices%20offer%20important%20cultural%20contexts%2C%20including%20passages%20from%20Marlowe's%20historical%20sources%20in%20Holinshed%20and%20Stow%2C%20Michael%20Drayton's%20very%20different%20poetic%20account%20of%20Edward's%20reign%2C%20a%20selection%20of%20early%20modern%20versions%20of%20the%20tradition%20of%20amity%20(or%20friendship%20between%20men)%2C%20and%20Renaissance%20legal%20and%20moral%20descriptions%20of%20sodomy.%26%2334%3B%20(Ian%20McAdam%20)%20--Ian%20McAdam%0A%20%20%20%20%20%20%3Cem%3E--This%20text%20refers%20to%20an%20out%20of%20print%20or%20unavailable%20edition%20of%20this%20title.%3C%2Fem%3E%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%0A%20%20%20%20%0A%20%20%20%20%20%20%20%3Ch3%20class%3D%22productDescriptionSource%22%3EFrom%20the%20Back%20Cover%3C%2Fh3%3E%0A%20%20%20%20%20%20%20%3Cdiv%20class%3D%22productDescriptionWrapper%22%3E%0A%20%20%20%20%20%20%20Depicting%20with%20shocking%20openness%20the%20sexual%20and%20political%20violence%20of%20its%20central%20characters'%20fates%2C%20Edward%20the%20Second%20broke%20new%20dramatic%20ground%20in%20English%20theatre.%20The%20play%20charts%20the%20tragic%20rise%20and%20fall%20of%20the%20medieval%20English%20monarch%20Edward%20the%20Second%2C%20his%20favourite%20Piers%20Gaveston%2C%20and%20their%20ambitious%20opponents%20Queen%20Isabella%20and%20Mortimer%20Jr.%2C%20and%20is%20an%20important%20cultural%2C%20as%20well%20as%20dramatic%2C%20document%20of%20the%20early%20modern%20period.%20%20%20This%20modernized%20and%20fully%20annotated%20Broadview%20Edition%20is%20prefaced%20by%20a%20critical%20but%20student-oriented%20introduction%20and%20followed%20by%20ample%20appendix%20material%2C%20including%20extended%20selections%20from%20Marlowe's%20historical%20sources%2C%20texts%20bearing%20on%20the%20play's%20complex%20sexual%20and%20political%20dynamics%2C%20and%20excerpts%20from%20contemporary%20poet%20Michael%20Drayton's%20epic%20rendition%20of%20Edward%20the%20Second's%20reign.%0A%20%20%20%20%20%20%3Cem%3E--This%20text%20refers%20to%20an%20alternate%20%0A%0A%0A%0A%0A%0A%0A%3Ca%20href%3D%22http%3A%2F%2Fwww.amazon.com%2Fgp%2Fproduct%2F1551119102%2Fref%3Ddp_proddesc_2%2F185-4427774-4886833%3Fie%3DUTF8%26n%3D283155%22%20class%3D%22product%22%3EPaperback%3C%2Fa%3E%0A%20edition.%3C%2Fem%3E%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%3Cdiv%20class%3D%22seeAll%22%3E%0A%0A%0A%0A%0A%3Ca%20href%3D%22http%3A%2F%2Fwww.amazon.com%2Fgp%2Fproduct%2Fproduct-description%2F0802151477%2Fref%3Ddp_proddesc_0%2F185-4427774-4886833%3Fie%3DUTF8%26isInIframe%3D1%26n%3D283155%26s%3Dbooks%22%3ESee%20all%20Editorial%20Reviews%3C%2Fa%3E%0A%3C%2Fdiv%3E%20%20%3C%2Fdiv%3E%0A%3C%2Fdiv%3E%0A%0A%0A%0A%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fbody%3E%0A%3C%2Fhtml%3E%0A";
    obj.onloadCallback = onloadCallback;
    obj.needWidthAdjust = needWidthAdjust;
    var iframe = obj.createIframe (obj.iframeId, obj.iframeload);
    var wrapper = d.getElementById(obj.appendTo);
    wrapper.innerHTML = '';

    var anchor = d.createElement('div');
    anchor.id = 'biss-product-description-and-details';
    wrapper.appendChild(anchor);

    iframe = wrapper.appendChild(iframe);
    var doc;
    try {
      doc = iframe.contentWindow.document;
    } catch (e) {
      try {
        doc = iframe.contentDocument;
      } catch (ex) {}
    }
    if (doc && doc.open) {
      obj.doc = doc;
      doc.open().write(decodeURIComponent(iframeContent));
      doc.close();
    }
  }
  obj.resizeIframe = function() {
    if(hasCustomWidth) {
      setIframeWidth(obj.iframeId);
    }
    nTries = 1;
    setIframeHeight(obj.iframeId);
  }
}).apply(ProductDescriptionIframeResize);

amznJQ.onCompletion('amznJQ.criticalFeature', function () {
  amznJQ.available('jQuery',function(){
    setTimeout(function() {
      var iframeLoadFn = function() {
        var $ = jQuery;
        $("#pd-available").trigger("pdAvailable");
        $(window).resize(ProductDescriptionIframeResize.resizeIframe);
      };
      ProductDescriptionIframeResize.initialize(iframeLoadFn, 1);
    }, 50);
  });
});
</script>




e-col%20%7B%0A%20%20%20%20width%3A%2098.8827%25%3B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20.last%20%7B%0A%20%20%20%20margin-right%3A0%3B%0A%7D%0A%0A%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20100%25%3B%0A%20%20%20%20min-width%3A%20895px%3B%0A%7D%0A%0A*%20html%20%23productDescription%20.aplus%20%7B%0A%20%20%20%20width%3A%20expression((document.body.clientWidth%20%3C%20936)%20%3F%20%22895px%22%20%3A%20%22100%25%22%20)%3B%0A%7D%0A%0A%20%20%20%20%3C%2Fstyle%3E%0A%20%20%20%20%3Cbase%20target%3D'_top'%20%2F%3E%0A%20%20%3C%2Fhead%3E%0A%20%20%3Cbody%3E%0A%20%20%3Cdiv%20id%3D%22pdIframeContent%22%3E%0A%0A%0A%0A%0A%20%0A%0A%0A%0A%0A%3Cdiv%20id%3D%22biss-product-description-and-details%22%3E%3C%2Fdiv%3E%0A%0A%3Cdiv%20class%3D%22bucket%22%20id%3D%22productDescription%22%3E%0A%20%20%3Ch2%3EEditorial%20Reviews%3C%2Fh2%3E%0A%0A%3Cdiv%20class%3D%22content%22%3E%0A%0A%0A%20%20%20%20%0A%20%20%20%20%20%20%20%3Ch3%20class%3D%22productDescriptionSource%22%3EReview%3C%2Fh3%3E%0A%20%20%20%20%20%20%20%3Cdiv%20class%3D%22productDescriptionWrapper%22%3E%0A%20%20%20%20%20%20%20%3CDIV%3E%3Cdiv%3E%3Cp%3E%20%20%20%20%3CI%3E%22Make%20good%20use%20of%20this%20book!%20It%20will%20help%20you%20to%20improve%20your%20work.%3C%2FI%3E%22%3C%2Fp%3E%3C%2Fdiv%3E%26%238212%3BDavid%20Naef%2C%20Creative%20director%2C%20Design%20Management%20Visionaer%3CBR%3E%3CBR%3E%3Cdiv%3E%3Cp%3E%20%20%20%20%3CI%3E%22This%20book%20is%20smooth%20and%20pleasing%20like%20Swiss%20chocolate%20and%20has%20the%20eloquence%20of%20a%20cherry%20blossom.%20It%26%238217%3Bs%20a%20must-read%20and%20real%20gem%20for%20everybody%20who%20is%20eager%20to%20learn%20about%20usability.%20%22%3C%2FI%3E%3C%2Fp%3E%3C%2Fdiv%3E%26%238212%3BMichael%20D.%20Trummer%2C%20Senior%20Engagement%20Manager%2C%20Appway%2C%20Inc.%3CBR%3E%3CBR%3E%3Cdiv%3E%3Cp%3E%20%20%20%20%3CI%3E%3CB%3E%26%238220%3BDesigned%20for%20Use%20%3C%2FB%3Edistills%20Lukas%26%238217%3Bs%20brilliant%20insight%20into%20the%20much%20neglected%20area%20of%20usability%2C%20UX%2C%20and%20UI%20design.%20An%20essential%2C%20authoritative%2C%20and%20enlightening%20read.%3C%2FI%3E%22%3C%2Fp%3E%3C%2Fdiv%3E%26%238212%3BPaul%20Neave%2C%20Interaction%20Designer%2C%20Neave%20Interactive%3C%2Fdiv%3E%0A%20%20%20%20%20%20%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%0A%20%20%20%20%0A%20%20%20%20%20%20%20%3Ch3%20class%3D%22productDescriptionSource%22%3EAbout%20the%20Author%3C%2Fh3%3E%0A%20%20%20%20%20%20%20%3Cdiv%20class%3D%22productDescriptionWrapper%22%3E%0A%20%20%20%20%20%20%20Lukas%20Mathis%20works%20as%20a%20developer%20and%20user%20interface%20designer%20for%20Swiss%20software%20company%20Numcom%20Software%2C%20creator%20of%20Appway%2C%20a%20process%20management%20software.%20He%20writes%20essays%20on%20design%20and%20usability%20at%20ignorethecode.net%2C%20and%20he%20writes%20about%20video%20games%20for%20wisegamers.ch%2C%20a%20Swiss%20gaming%20news%20site.%20His%20essays%20on%20design%20have%20been%20published%20on%20sites%20like%20UX%20Magazine%2C%20TidBITS%2C%20and%20Splashnology.%20He%20has%20created%20a%20number%20of%20online%20tools%20for%20UI%20design%2C%20among%20them%20iphonemockup.net%2C%20a%20collaborative%20iPhone%20UI%20mockup%20designer.%0A%20%20%20%20%20%20%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fdiv%3E%0A%3C%2Fdiv%3E%0A%0A%0A%0A%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fbody%3E%0A%3C%2Fhtml%3E%0A";
    obj.onloadCallback = onloadCallback;
    obj.needWidthAdjust = needWidthAdjust;
    var iframe = obj.createIframe (obj.iframeId, obj.iframeload);
    var wrapper = d.getElementById(obj.appendTo);
    wrapper.innerHTML = '';

    var anchor = d.createElement('div');
    anchor.id = 'biss-product-description-and-details';
    wrapper.appendChild(anchor);

    iframe = wrapper.appendChild(iframe);
    var doc;
    try {
      doc = iframe.contentWindow.document;
    } catch (e) {
      try {
        doc = iframe.contentDocument;
      } catch (ex) {}
    }
    if (doc && doc.open) {
      obj.doc = doc;
      doc.open().write(decodeURIComponent(iframeContent));
      doc.close();
    }
  }
  obj.resizeIframe = function() {
    if(hasCustomWidth) {
      setIframeWidth(obj.iframeId);
    }
    nTries = 1;
    setIframeHeight(obj.iframeId);
  }
}).apply(ProductDescriptionIframeResize);

amznJQ.onCompletion('amznJQ.criticalFeature', function () {
  amznJQ.available('jQuery',function(){
    setTimeout(function() {
      var iframeLoadFn = function() {
        var $ = jQuery;
        $("#pd-available").trigger("pdAvailable");
        $(window).resize(ProductDescriptionIframeResize.resizeIframe);
      };
      ProductDescriptionIframeResize.initialize(iframeLoadFn, 1);
    }, 50);
  });
});
</script>




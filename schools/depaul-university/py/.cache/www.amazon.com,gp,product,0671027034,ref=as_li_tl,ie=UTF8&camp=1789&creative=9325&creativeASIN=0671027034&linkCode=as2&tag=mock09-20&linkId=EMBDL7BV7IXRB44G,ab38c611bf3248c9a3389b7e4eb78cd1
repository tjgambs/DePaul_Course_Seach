  if (typeof CSMReqs !== 'undefined') {
     if (CSMReqs.hasOwnProperty('af')) {
        CSMReqs.af.c = 1;
     }
     if (CSMReqs.hasOwnProperty('cf')) {
        CSMReqs.cf.c = 1;
     }
  }
</script>


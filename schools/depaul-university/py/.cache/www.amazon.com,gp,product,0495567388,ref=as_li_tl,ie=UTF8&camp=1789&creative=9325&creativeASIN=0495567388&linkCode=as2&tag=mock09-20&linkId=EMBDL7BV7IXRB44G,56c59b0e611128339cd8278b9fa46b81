<script type="text/javascript">
P.when('A').register("bookDescriptionIframe", function(A) {
  var bookDescriptionData = {
      'encodedDescription' : "BUSINESS%20AND%20PROFESSIONAL%20COMMUNICATION%20IN%20THE%20GLOBAL%20WORKPLACE%2C%20Third%20Edition%2C%20provides%20an%20introduction%20to%20business%20and%20professional%20speaking%20course%20at%20two-%20and%20four-year%20institutions.",

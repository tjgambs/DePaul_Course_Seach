           }
 
        }

		if( typeof A9_SL_CSM_JS != 'undefined' ){
		  A9_SL_CSM_JS($, reformatType);
		}

        function dumpHtml( obj, newAdsHtml, topHtml, bottomHtml, divParent ) {
              var i = 0;
              var completeAdsHtml = '';
              for( x in obj ) {
                  var adChunks = newAdsHtml.split( adPropertiesDelimiter );
                  for( var j = 0; j<adChunks.length; j++ ) {
                      if( typeof obj[i][adChunks[j]] != "undefined" ) {
                          adChunks[j] = obj[i][adChunks[j]];
                      }
                  }
                  completeAdsHtml += adChunks.join("");
                  i++;
              }
              newAdsHtml = topHtml + completeAdsHtml + bottomHtml;
              $('#' + divParent).replaceWith(newAdsHtml);
        }
	});

	P.when('jQuery','ready').execute(function($){
	    var jsonAds = '[{"redirectUrl":"http://rd.a9.com/srv/redirect/?info=ALwsPDmw6iXZrdlnauybCdL5Bm3ybw-M.JMC2mcODQUx01OsUYg5jlJmbl6QDHq7r3aXYaozf.vb9BKYrCrlDfODtPJs4fahvkBE0Heg6BAGsQ8RBZlLZV9IsTlGwtemNFNYYOOc07HXSqjsSySRdbFKnhpLhH9QXAMYxYOXWY0dMP-oi-2F3gGF0RjZVpOyrtepxDYoxEumskKeBBwnrJfDWp-YixXYlesKLtuvM1jpSb9H294VA65jVn90zpmbHPSbJcTvmg7XOc5ryO72dOjHilQwCgIyCgM0Gh6vB4FraZ6jgu3GoxoD0yYZZgw72IK5n7BKO0GdWEVg4LrNVyGHeovcjYDFLCyRLYnxWOT.lH9Q.t5ylf4bm7yHtLpHdmaSo8fOR9-5gijmV5uc2ZchuuKpGkHWryWYiilvdqhjES9DGs90Y.Ec1QctoJd0hyZ9HPFh.FL2jnhFbkFCb7-MdNvHCHZ7FofejDLVlrn9ZUd1WUJi6xVK9npNRb-qQZYCeEutmNm1aypVmuZYHe6HsIC3lxAMxcx4wXao8fASRlUaevwGh6xEKJghkC0xKQVaY566mjeQczDPFBetVMAmPcnTIV7OmwIkDPgOl2BIY20rMOKoY1FtqLo8uj1uIRp5VX4c54.bjkkfl3Kwa5M-QT6H1T2CsU9MqV2sleJyfGYzYnEULuyG-CP4jRWX9xmLVaVQMPQbr6g67Sm4RNhkxLCESGJoPQVjhkiBJ0ZozXYLyFByVR.Q75IkB9fkKOGADJj0ZAZQ9iUcjsYfHCZMdO-vKNrMd9mlzQKR6Irm2eXp9lUi-PSLMqKVkgle0PJq6wrC1N5Uh6fGeyJisHlVAJw36dvm0NyrZYeeYlvM8c22PNScbVBGnDO3IbPat9gAxex4-OcOlrKh7Oxg.KJie4QKFiQbFmb9XSSZvYyUw68kP2DYbOUeVztcc14ccskvbvgV5M4c6REL2SqotvDJPLkU28S.V8-SuTB2cgBjCSyqkfbyOyFZ.IccnlJ-edB9Xby5oqH9qbeVoZWYED05aQ.DpF1KX5f-kxA5WDw.Tuhs-XJh6nQSOufqn1gq9j6xwCmjZwToU7UFuMKtE2tOEJkHLEGRKGQufMp0ng328LuDDhI0V7mwNI5fGsJBok-n6ppvs2KJnrzGNmu6SNBWgoHK73D2MbsGWq3wLFb4XM3cPiFzqnw_&awt=1&s=","visibleUrl":"www.momastore.org/","provider":"g","description":"Reflect Your Modern Style. Shop the MoMA Store Collection Today!","title":"<b>Eight Propositions</b> | MoMA"}]';
	    var feedbackFormAvailable = 0;
	    function showSLF() {
	         if(!feedbackFormAvailable) {
	           // Make a request to the handler with new ads 
	           $.post('/gp/product/aui_feedback_form_handler.html', { 'sid': '179-2355227-1790008','jsonAds' : escape(jsonAds) } ,function(data) {
	               $('#FeedbackFormDiv').html(data);
	               $('#FeedbackFormDiv').show();
	               $('#ShowFeedbackLinkDiv').hide();
	               $('#ResponseFeedbackLinkDiv').hide(); 
	               feedbackFormAvailable = 1;
	           });
	         } else {
	           $('#reports-ads-abuse-form').show();
	           $('#FeedbackFormDiv').show();
	           $('#reports-ads-abuse').show();
	           $('#ShowFeedbackLinkDiv').hide();
	           $('#ResponseFeedbackLinkDiv').hide(); 
	         }
	    }
	    $('#LetUsKnowLink').click(showSLF);
	    $('#ShowFeedbackLinkDiv').css('opacity','1');
	    $('#ShowFeedbackLinkDiv').css('pointer-events','auto');
	});


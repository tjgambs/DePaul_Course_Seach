l%20section%20is%20a%20special%20six-piece%20tribute%20to%20man%20who%20himself%20claimed%20to%20be%20the%20best%20of%20the%20best%20Muhammad%20Ali.%20%3CBR%3ECopyright%201999%20Reed%20Business%20Information%2C%20Inc.%0A%20%20%20%20%20%20%3Cem%3E--This%20text%20refers%20to%20an%20out%20of%20print%20or%20unavailable%20edition%20of%20this%20title.%3C%2Fem%3E%0A%20%20%20%20%20%20%3Cdiv%20class%3D%22emptyClear%22%3E%20%3C%2Fdiv%3E%0A%20%20%20%20%20%20%3C%2Fdiv%3E%0A%3Cdiv%20class%3D%22seeAll%22%3E%0A%0A%0A%0A%0A%3Ca%20href%3D%22http%3A%2F%2Fwww.amazon.com%2Fgp%2Fproduct%2Fproduct-description%2F0395945143%2Fref%3Ddp_proddesc_0%2F178-7042146-2636553%3Fie%3DUTF8%26isInIframe%3D1%26n%3D283155%26s%3Dbooks%22%3ESee%20all%20Editorial%20Reviews%3C%2Fa%3E%0A%3C%2Fdiv%3E%20%20%3C%2Fdiv%3E%0A%3C%2Fdiv%3E%0A%0A%0A%0A%20%20%3C%2Fdiv%3E%0A%20%20%3C%2Fbody%3E%0A%3C%2Fhtml%3E%0A";
    obj.onloadCallback = onloadCallback;
    obj.needWidthAdjust = needWidthAdjust;
    var iframe = obj.createIframe (obj.iframeId, obj.iframeload);
    var wrapper = d.getElementById(obj.appendTo);
    wrapper.innerHTML = '';

    var anchor = d.createElement('div');
    anchor.id = 'biss-product-description-and-details';
    wrapper.appendChild(anchor);

    iframe = wrapper.appendChild(iframe);
    var doc;
    try {
      doc = iframe.contentWindow.document;
    } catch (e) {
      try {
        doc = iframe.contentDocument;
      } catch (ex) {}
    }
    if (doc && doc.open) {
      obj.doc = doc;
      doc.open().write(decodeURIComponent(iframeContent));
      doc.close();
    }
  }
  obj.resizeIframe = function() {
    if(hasCustomWidth) {
      setIframeWidth(obj.iframeId);
    }
    nTries = 1;
    setIframeHeight(obj.iframeId);
  }
}).apply(ProductDescriptionIframeResize);

amznJQ.onCompletion('amznJQ.criticalFeature', function () {
  amznJQ.available('jQuery',function(){
    setTimeout(function() {
      var iframeLoadFn = function() {
        var $ = jQuery;
        $("#pd-available").trigger("pdAvailable");
        $(window).resize(ProductDescriptionIframeResize.resizeIframe);
      };
      ProductDescriptionIframeResize.initialize(iframeLoadFn, 1);
    }, 50);
  });
});
</script>




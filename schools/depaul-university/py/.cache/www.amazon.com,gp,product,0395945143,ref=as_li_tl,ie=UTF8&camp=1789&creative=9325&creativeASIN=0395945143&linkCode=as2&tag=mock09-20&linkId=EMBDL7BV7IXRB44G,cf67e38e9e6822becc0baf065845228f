    }
    var data = {
        
        'litbData' : {
            'hasLitb' : false,
            'litbReftag' : "sib_dp_pt"
        },
        'audibleData' : audibleData,
        'windowWidthThreshold' : 1150,
        'configWidths' : [200, 260],
        'holderRatio' : 0.75,
        'containerMargin' : 15,
        'flipLinkMinHeight' : 0,
        'frontImageWidth' : 227,
        'frontImageHeight' : 346,
        'frontImageAspectRatio' : 0.6560693641618497,
        'imageGalleryData' : [{"mainUrl":"http://ecx.images-amazon.com/images/I/71W9S4W4G6L.gif","dimensions":[310,475],"thumbUrl":"http://ecx.images-amazon.com/images/I/71W9S4W4G6L._AC_SX75_CR,0,0,75,75_.gif"}],
        'centerColMargin' : 40,
        'disableResize' : false
    };
    return data;
});

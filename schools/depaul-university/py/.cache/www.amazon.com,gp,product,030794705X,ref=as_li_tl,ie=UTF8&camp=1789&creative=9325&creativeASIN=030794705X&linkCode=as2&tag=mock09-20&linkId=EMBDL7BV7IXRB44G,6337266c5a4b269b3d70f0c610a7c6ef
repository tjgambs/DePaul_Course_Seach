        <script type="text/javascript">
            P.now("LUXContext").execute(function(LUXContext) {
                if (!LUXContext) {
                    P.declare("LUXContext", {
                        isRecognizedCustomer: false,
                        deviceType: "web",
                        isOneClick: false,
                        clientPage: "DPX"
                    });

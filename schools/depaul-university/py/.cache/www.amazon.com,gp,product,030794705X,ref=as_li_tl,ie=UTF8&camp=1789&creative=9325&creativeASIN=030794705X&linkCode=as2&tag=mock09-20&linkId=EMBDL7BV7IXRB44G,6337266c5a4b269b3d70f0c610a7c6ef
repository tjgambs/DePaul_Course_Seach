            P.when("LUXController").execute(function(LUXController) {
                LUXController.init({
                    addressLabel : "",
                    addressId    : "",
                    obfuscatedId : "",
                    countryCode  : "US",
                    zipCode      : "60090",
                    state        : "IL",
                    city         : "WHEELING",
                    district     : "",
                    locationType : "IP2LOCATION"
                });
            });

            P.when("jQuery", "LOCUX_DETALPAGE_51079_T1").execute(function($, LOCUX_DETALPAGE_51079_T1) {
                $("#unifiedLocationCountrySelectionDiv").removeClass("aok-hidden");
                $(".lux-country-selector-divider").removeClass("aok-hidden");
            });
        </script>

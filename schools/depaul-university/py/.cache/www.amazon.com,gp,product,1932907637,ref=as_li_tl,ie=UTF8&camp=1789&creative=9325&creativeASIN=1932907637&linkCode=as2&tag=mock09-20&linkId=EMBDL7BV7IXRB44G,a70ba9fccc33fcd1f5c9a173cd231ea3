<script type="text/javascript">
P.when('A').register("bookDescriptionIframe", function(A) {
  var bookDescriptionData = {
      'encodedDescription' : "%3CDIV%3EThe%20Hollywood%20Standard%20describes%20in%20clear%2C%20vivid%20prose%20and%20hundreds%20of%20examples%20how%20to%20format%20every%20element%20of%20a%20screenplay%20or%20television%20script.%20A%20reference%20for%20everyone%20who%20writes%20for%20the%20screen%2C%20from%20the%20novice%20to%20the%20veteran%2C%20this%20is%20the%20dictionary%20of%20script%20format%2C%20with%20instructions%20for%20formatting%20everything%20from%20the%20simplest%20master%20scene%20heading%20to%20the%20most%20complex%20and%20challenging%20musical%20underwater%20dream%20sequence.%20This%20new%20edition%20includes%20a%20quick%20start%20guide%2C%20plus%20new%20chapters%20on%20avoiding%20a%20dozen%20deadly%20formatting%20mistakes%2C%20clarifying%20the%20difference%20between%20a%20spec%20script%20and%20production%20script%2C%20and%20mastering%20the%20vital%20art%20of%20proofreading.%20For%20the%20first%20time%2C%20readers%20will%20find%20instructions%20for%20formatting%20instant%20messages%2C%20text%20messages%2C%20email%20exchanges%20and%20caller%20ID.%3C%2Fdiv%3E",
      'staticHeight' : false,
      'numLines' : 0,
      'featureDiv' : "bookDescription_feature_div",

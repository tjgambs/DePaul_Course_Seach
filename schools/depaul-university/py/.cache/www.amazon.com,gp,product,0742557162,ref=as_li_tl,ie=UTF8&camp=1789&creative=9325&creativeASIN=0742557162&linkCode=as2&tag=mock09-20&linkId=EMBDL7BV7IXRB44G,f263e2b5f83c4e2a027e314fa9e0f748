<form style="display: none;">
  <input type="hidden" id="rwol-display-called" value="0">
</form>

    <script type="text/javascript">
      window.$Nav && $Nav.when('$').run('RetailWebsiteOverlay', function($) {
        var data = {"pageType":"Detail","path":"/gp/product/0742557162","isAUI":"","linkId":"EMBDL7BV7IXRB44G","linkCode":"as2","subPageType":"Glance","ASIN":"0742557162","tag":"mock09-20","creativeASIN":"0742557162","creative":"9325","camp":"1789"};

        if (window.sessionStorage) {
          if (!document.referrer) {
            var lastPageSet = sessionStorage.getItem("rwolLastPageSet");
            if (lastPageSet && parseInt(lastPageSet, 10) > (Date.now() - 30000)) {
              data.referrer = sessionStorage.getItem("rwolLastPage");
            }
          }
          sessionStorage.setItem("rwolLastPage", window.location.href);
          sessionStorage.setItem("rwolLastPageSet", Date.now());
        }

        data.referrer = data.referrer || document.referrer;

        if (data.referrer) {
          if (data.referrer.match(/^https?:\/\/([^\/]+\.)?amazon\.com(\/|$)/i)) {
            return;
          }
        }

        data.queryString = window.location.search;

        var displayCalled = $('#rwol-display-called');
        if (displayCalled.val() !== '0') {
          return;
        }
        displayCalled.val(1);

        var buildUpdate = function(placementId, priority) {
          var executed = false;
          return function() {
            if (executed) {
              return;
            }

            $.ajax({
              url: '/gp/overlay/update.html',
              type: 'POST',
              data: {placementId: placementId, priority: priority}
            });
            executed = true;
          };
        };

        $.ajax({
          url: '/gp/overlay/display.html',
          type: 'POST',
          data: data,
          dataType: 'json',
          success: function(data) {
            if (data && data.content) {
              var update = buildUpdate(data.placementId, data.priority);

              $('body').append(data.content);
              if (!(data.displayLogic || "").match('client')) {
                update();
              }
              $(window).bind('rw:overlay:displayed', update);

            }
          },
          error: function(xhr, message, error) {
              console.debug(arguments); 
          }
        });
      });
    </script>




    
    
    
    
    
    












































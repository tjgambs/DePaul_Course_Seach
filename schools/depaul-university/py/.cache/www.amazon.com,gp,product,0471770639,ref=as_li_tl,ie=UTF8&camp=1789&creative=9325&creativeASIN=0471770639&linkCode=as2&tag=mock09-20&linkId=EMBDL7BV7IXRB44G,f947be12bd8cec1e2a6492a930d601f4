            'hasLitb' : true,
            'litbReftag' : "sib_dp_pt"
        },
        'audibleData' : audibleData,
        'windowWidthThreshold' : 1150,
        'configWidths' : [200, 260],
        'holderRatio' : 0.75,
        'containerMargin' : 15,
        'flipLinkMinHeight' : 25,
        'frontImageWidth' : 260,
        'frontImageHeight' : 329,
        'frontImageAspectRatio' : 0.790273556231003,
        'imageGalleryData' : [{"mainUrl":"http://ecx.images-amazon.com/images/I/51k9OHb4woL.jpg","dimensions":[395,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/51k9OHb4woL._AC_SX75_CR,0,0,75,75_.jpg"},{"mainUrl":"http://ecx.images-amazon.com/images/I/51KVQzerSUL.jpg","dimensions":[395,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/51KVQzerSUL._AC_SX75_CR,0,0,75,75_.jpg"},{"mainUrl":"http://ecx.images-amazon.com/images/I/41JlONorbFL.jpg","dimensions":[375,500],"thumbUrl":"http://ecx.images-amazon.com/images/I/41JlONorbFL._AC_SX75_CR,0,0,75,75_.jpg"}],
